def print_pattern_114():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                +++++++++++++                                       
                                              +++****+++****++++                                    
                                           +*%%SS##*%%?*+++****+++                                  
                                        : ?#######S++###S?*++*****++                                
                                       +%##%%SSS#S:%?#####S%*+******+                               
                                     +?###%*?SSSSS S######*SS?++*****++                             
                                    *S##SSS##SSSSS%SS##S+  S##%*+******+                            
                                   *##SSSSS#?+S#SS#S %#SS?%####S*+******+                           
                                  *S#SSSSS#%+S#SSSSS*:%#S######@S*+******+                          
                                 ?S*SSSSSSSSSS*+S####%S% S#####SSS*+******+                         
                                +#?*SS :%#SS#%?SSS%%SS#S:?#####+:?S*+******+                        
                               +S# %S+%%SSSSSS#S#*::?:*#+*######S##S*+******+                       
                              +S##*%# %#SSSSSSS#SS%+S? S*?#S########?+******++                      
                              ?#SSSS#**%#SSSSS#SSS#+%#SS:?#S#########*+******+                      
                             +?#SSSSSS% %#SSSSS %##%%##S:%#SS########S+*******+                     
                             *+#SSSSSS#? %SSS#* SSSSSS#%+SSSS#########?+*******+                    
                            *% %#SSSSSS#%*%###?%#SSSSSSSSSSSS#####%%###*+******+                    
                            *#**#SSSSS###########SSSSSSSSSSSSS#### +@##?+*******+                   
                            %#%*#S#SS#############SSSSSSSSS##SS+S@++@##S*+******+                   
                           +##SS#%*S###############SSSSSSS#%%#?:#@??##@#?+******++                  
                          +*S##SSS ?####S?S#########SSSSSS*+%#++######*#S+*******+                  
                          + %??SS#SS##S#S ?#%?+?%####SSS#%*S##?S#####+:##*+******++                 
                          ?SS% %#S###**@# **+++++*%###SSSS#SSS#######+S##%+*******+                 
                          S@##SSSS### S##*++*****++?##SSSSSSSSS##########S+*******+                 
                         +###SSSS#######?+*******++ ?##SSSSSSSS###########*+******+                 
                          S##SSSS#??##S++*******+   +%#SSSSSSSS###########?+******++                
                         ?##SSS#%%%S### +******++    :##SSSSSSS##########S::+******+                
                         %##SSS*%*####%+*******+      %#SSSSSSS#S%########?+*******+                
                         S@#S#*+S##*S#*+******+       *#SSSSSS#@%:S########*+******+                
                         *?S#SS#S#S*#S*+******+       +S#SSSSS#SS:*########*+******++               
                        +SSSSSSSS####%+*******+        %#SSSSSS+%?+########?+******++               
                        +###SSSSS#SS#?+*******+        ?#SSSSS+?###########?+******++               
                        +##SSSSSS#S*#*+******++        *#SSSSS%############?+*******+               
                        +##%?#SSSS###++******++        +#SSSSS#############?+*******+               
                        +##%?##SSS###++******++        +SSSSSS############@* *******+               
                        +###SS?SSSS##+ ******++        +SSSSS#############@* *******+               
                        +####S SSSS##* +******+        +SSSSS#####SS######@*+*******+               
                        +S%##S+%#SS##*  +*****+        +SSSSS####?+?#######?+*******+               
                        +%?##SSSSSSS#%   *****+        *SSSSS###S*S########?+*******+               
                         ?%##SSSSSSS#S   +****+        +SSSS############?S#?+******++               
                         ?###SSSSSSSS#+   +****+        SSSS###########S+S#?+******+                
                         *@@S+SSSSSSS#%    +***+       *#SSS###########?*##*+******+                
                         +?%S:+#SSSSSSS+    +***+     +SSSS#####S?#####SS##*+******+                
                          ++@% SSSSSSS#%     +***+    %#SSS####*+?########S*+******+                
                          **@S:?#SSSSSS#*     ++**++ *?+#SS###S?%#########%+******++                
                          *###+SSSSS%*S#S+      ++++*SS:SS###?+S++?#######?+******+                 
                          +###S#SS#%+%SS#S         +%##?%###@? ##SS#######*+******+                 
                           %####SSSS??#SSSS?    : +S#SSS*?S#@? ##########S+*******+                 
                           *####SSSS#SSSSS#S: ? : +%#SSS???#S+?##########%+******+                  
                            +S###SSSSSSSSSS#??S%S%?SSSS#S+:?#:%##########?+******+                  
                            +S####SS*SSSS*+S%%##S#SSSSS#S*S#@* ######?%#S*******++                  
                             *#####%?#SS#SSS*+#SSSSSSS###@###S*######*:%?+******+                   
                             +#####SS#S#%?*S? #SSSSSS#################SS*+*****++                   
                             +S######SS#+* +S%#SSSS############S %#####?+******+                    
                              ?%S####??#+%S?%SSSSS#?S#########%:+S####S*+*****++                    
                               ?*###S?##SS%?*SSSS##?%###S#####S%######?+******+                     
                                %#S##S##@? ?S######S*##*?##########S*********+                      
                                *#%?@*+##+:#########%#S+############?+******+                       
                                 ?%+#%,%#S*+###########S############?+*****++                       
                                 +**S#?%##?*#######################?+******+                        
                                   ?######?#######################%+*****++                         
                                    ?S?####################### ?#?++****++                          
                                     **###########%##########++#?+*****++                           
                                      +%####S**S##*S#########SS*+*****+                             
                                        *S########?###S %##S%?++****++                              
                                          *%##########@?+#%++++***++                                
                                            +*%SS######S?**+****++                                  
                                                 ******+++**++++                                    
                                                    ++++++++ 
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_115():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                ++++++++++++                                        
                                              +++****+++****+++                                     
                                           +*%%SS##S*%%?*++****+++                                  
                                        : ?########%:?@#S%*++*****++                                
                                       *%##%?S#SS#% S?#####S?++****++                               
                                     +%###S?*%#SS#? #######?S%*+*****++                             
                                    ?##SSSS##SSSSSSSSS##S+  ##S?++*****+                            
                                   ?##SSSSSSS+?#SSS#% %##S*%####%++*****+                           
                                 +?%#SSSSSSS*?#SSSSSS+:S##@######%*+*****+                          
                                 %#+SSSSSSSSS#%+*S####?S%+########%++*****+                         
                                ?#**S#*:*#SSSS%%SS%%%SS#S:?#####+:?%++*****+                        
                               *## ?#*%S%SSSSS##SS+: *:*#*+######%S#%+******+                       
                              *###*?#+*##SSSSSSS#SS%+S?:%??##########?+******+                      
                             +##SSSS#?+?SSSSSSSS%SSS+%#S# *#S########S*+*****++                     
                             *?#SSSSSSS++##SSS#% SS#?%##S:%#S#########%+******+                     
                            ***#SSSSSS#S *%SSSS *#SSSSSSS+SSSS#########?+******+                    
                            S? %#SSSSSS#S**###S*SSSSSSSSSSSSSS#####S%##S*+*****++                   
                            S#+?#SSSSSS##########SSSSSSSSSSSSS####@* ###?+******+                   
                           +##%?#S#SS##############SSSSSSSSSS###*%@? ###S*+*****+                   
                          +%#SSS#?*S################SSSSSSS#S%#S:%@%+####?+******+                  
                          **#S#SSS ?####S?%##########SSSSSS?+?#?:######?SS+******+                  
                         ++ S+?SSSSS##S## *#%?+?%S####SSSSS*%S#%?#####?,S#*+******+                 
                         +SSS% SSS###**@#++*+++++*?###SSSSS##SS#######*?##%+******+                 
                         ?@#S#SSSS### S##?++******+*%##SSSSSSSS###########S*+*****+                 
                         ?##SSSSS#######?+*******++  S#SSSSSSSS############*+******+                
                         ?##SSSS##??##S++*******+    *S#SSSSSSS############?+******+                
                        +###SS##?%%###S +******+    + +#SSSSSSS###########S::+*****+                
                        *###SS%*%*####?+******+       +##SSSSSS###%########?++*****+                
                        ?@##SS+*###*##*+******+        %#SSSSSS###:?########*+******+               
                        *?%#SSS#S#%?#%+******++        *#SSSSSS##S+ ########?+******+               
                        %SS#SSSSS#S##?+******+         +S#SSSSS#**S:S#######?+******+               
                        %@##SSSSS#SS#*+******+          %#SSSS#*+###########?+******+               
                        %##SSSSSS#%*#*+******+          ?#SSSSS%S###########%+******+               
                        S##*SSSSSS##S+*******+          *#SSSSS#############?+******+               
                        S##*%##SSS##S+*******+          *#SSSSS############@* ******+               
                        %##SS%%#SS##S +******+          *SSSSS#############@* ******+               
                        %@###?+#SSS##  ******+          *#SSSS######%#######?+******+               
                        %SS##% SSSS##+ +*****+          *#SSSS####%+*#######%+******+               
                        ??S#SSSSSSS##*  +****++         ?#SSSS####?%########%+******+               
                        *?S##S#SSSSS#%   +****+         +SSSS############?%@?+******+               
                        +####SSSSSSS#S    ****+         +#SSS############+%#?+******+               
                        +#@#%*SSSSSSS#*   +****+      +:?#SSS###########S*##*+*****++               
                        +%%#?:?#SSSSS#S    ++**++      ?SSSS#####S?S#####S##*+*****+                
                         +:?@++SSSSSSS#?     +**++    +SSSSS####?+*########S*+*****+                
                         **%@?,%#SSSSSS#+     ++*++   %?+#SS###S%?S########%+******+                
                         +S##S+SSSS#%*S#S+      +++++?#%:%####S S*+*#######?+******+                
                          %###SSSS#%*%SS#S          *SS#?%####S:###########*+*****+                 
                          *####SSSSS??#SS#S?    :  ?#SSSS**%##S:##########S+******+                 
                           S@###SSSS#SSSSSS#+:*+::+*SSSSS??*S#+?##########%+******+                 
                           +?####SSSSSSSSSS#%*S%S%%?SSSS#S+:*@+*#######@##*+*****+                  
                            *#####SS?SSSS?+?S?S#S#SSSSSS#S*S##S,S#####?*SS+******+                  
                           + %#####S*SSSSSSSS %#SSSSSSS###@####?S#####% *?+*****+                   
                             ?@####S%#SSS%?%# %#SSSSSS##################S+******+                   
                             ?#######SS#? *:S?SSSSSS############S *#####?+*****+                    
                             +S%S####%?#?*#?%SSSSSSS?##########S:+S####S+******+                    
                              *%?####*S#SSS%+?#SSS##?%###%######%######*+*****+                     
                               +%#S##S#### *%#######%*##*?##########S+++*****+                      
                                %@%?@%+#@%,%@########%#S+############%+*****++                      
