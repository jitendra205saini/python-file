def print_pattern_29():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                            ++++++++++                                              
                                          +++*********++                                            
                                         ++*************+                                           
                                        +****************+                                          
                                       +*+++++************+:                                        
                                      +*+     +************+%*                                      
                                      ++      +************+%#                                      
                                     ++        +***********+*S?                                     
                                     ++        +************+?#S+                                   
                                    ++          +************+ #%                                   
                                    ++          +************+:S#?                                  
                                    ++          +*************+S#S+                                 
                                    ++          +************+*S##*                                 
                                    ++          +*************+%#@%                                 
                                   +*+          +*************+?##%+                                
                                   +*+          +*************+*#+:*+                               
                                   +*+          +*************+*#*+S+                               
                                   +**          +**************+SS #+                               
                                   +**          +**************+%#S#*                               
                                   +**          +**************+?##@?                               
