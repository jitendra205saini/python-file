                      +**************+*S#############     ?? #+%###+                                
                      +**************+*S#############     ?%?#%###S+                                
                      +***************+S###########S#*   +S#####?#%                                 
                      +***************+S###########*#?   +SS####+S%                                 
                      +***************+S###########*#?   ?SS#####?*                                 
                      +***************+%###SS#####S*#%  +%#######%:                                 
                      +***************+%##@**@#####S#% ++S##SS###S+                                 
                      ++**************+?### *@#######?+++SS%**S###*                                 
                       +**************+?### *@######@?:*%***%SS###+                                 
                       +**************+*###%S#######%S:?+ #######S+                                 
                       +**************+*##########*::# *+*#S#####%                                  
                       +**************+*S#########%* #*?###?*###?*                                  
                        +**************+S#########@?*@S##%%# S##?+                                  
                        +**************+%################S#%%###S+                                  
                        ++*************+?################*# +%##%                                   
                         +*************+*################?#*%?##*                                   
                         +**************+S###############S#%?*@#+                                   
                         ++*************+?##??######SS####S?%+@%                                    
                          +*************+*##S++S###?:S####%%% S*                                    
                           +************+*S#@*+####* #####SS#?%+                                    
                           +*************+?########S ######+S%*                                     
                            +************+*########S ##S###+S%                                      
                            ++************+%########S#S*#?#S#+                                      
                             +************+*###########?#?#@%                                       
                              +************+%###########??SS+                                       
                               +***********+*S#####+####S%*+                                        
                                +***********+*####%:#%##@%                                          
                                 ++**********+*S##S%S*##?+                                          
                                  ++**********++*S#S%%%*                                            
                                    ++**********+:**++                                              
                                      ++++*****+++                                                  
                                         +++++++  
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_16():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                          +++++++                                                   
                                       +++*******+++                                                
                                     +++**********+++  :                                            
                                   ++**********+ +*%S%?                                             
                                  ++***********+:?#@##@#S*                                          
                                 +*************+?#?%######?                                         
                                +++ +*********+?#S:%#######?                                        
                               ++    +*********###S######%%#+                                       
                              ++      ******* *##########%?@S+                                      
                              +       +******:*@#########%+#@%                                      
                             +        +*****+ S##########S #S#                                      
                            +         +****+*S########S?##*%:S*                                     
                            +         *****+?#########S:##S% S#+                                    
                           +         +*****+%#######SSS ##@%*@S+                                    
                           +         +****+*SS +####??#?+S#S?@#%                                    
                          +          +****+*#+:%####*%#@+S##*##S                                    
                          +          *****+%#:*@####?S##*%@# ##S                                    
                         +          +*****+S#+S######SS#SS#S*#SS+                                   
                         +          +****+*S#########:*#####S#+%*                                   
                         +         +*****+*#########%:*##*####%#S                                   
                        ++         +*****+?## S#####S%?*? #######+                                  
                        ++        +******+%@%,S########% S#######?                                  
                        +         +*******S@*:####@#####%########%                                  
                        +        +******+*##%?###S?S#############%                                  
                       ++        +******+*#######%:*###%%########%                                  
                       ++       +*******+?#############?*##%S####S+                                 
                       +*      +********+?#############?+S@?%@####+                                 
                       +*+    +*********+%@############*+%#S%?####+                                 
                       +*+  ++**********+%?###########S +?#@%*####*                                 
                       +***+*************%+S####?S####S++*########*                                 
                       +*****************S?%####+:##??%++*S#######*                                 
                       +***************+*S#######*S#S%%+*+%#######*                                 
                       +***************+*S##S########@%+*+%#######*                                 
                       +***************+*###+%########?+*+?#######?                                 
                      ++***************+*###?*########* ++%S######?                                 
                      ++***************+*###S%########+ +*%*######?                                 
                      ++***************+*#############+  +%%######*                                 
                      ++***************+*#############+  **?######*                                 
                      ++***************+*#############+  *+ %#*###*                                 
                       +***************+*#############+  +%+%S+###*                                 
                       +***************+*############@+  +S?SS%###+                                 
                       +***************+*S###########S?  *S###@%%#                                  
                       +***************+*S##########%?%  ?#S###%*S                                  
                       +***************+*S##########%%% +%######%*                                  
                       +****************+S###SS#####?%S *SS######                                   
                       +****************+S###+?@####S#S+*S#SSS###*                                  
                       +****************+%###:?@######% *SS??*###%                                  
                        +***************+%###:?######@%:S+*?SS###?                                  
                        +***************+?###%S######%S   #######*                                  
                        +***************+?########## ,%+:*#S####S+                                  
                        +***************+*##########?:S*%#S?+##S*+                                  
                        ++**************+*########### #S#S?#+S##*                                   
                         +***************+S################%?###*                                   
                         +***************+%##############%?* ?##+                                   
                         ++**************+?##############S%%??#S+                                   
                          +**************+*###############SS?*@%                                    
                          +**************+*S##*S@####SS####?%+#*                                    
                           +**************+%##? ?###S:%####?S %+                                    
                           +**************+?### ?###%:S####%#??                                     
                            +*************+*######### S###@*?%+                                     
                            +**************+%######## SSS##??%                                      
                             +*************+*########S#S?S%#S+                                      
                              +************+*S#########S%S?@?                                       
                               +************+?##########S*%%                                        
                               ++************+%#####+####S*+                                        
                                ++***********+*####%:S%##%+                                         
                                 ++***********+*S###%%?#%+                                          
                                   +***********++?S#%%%*                                            
                                    ++***********+:*++                                              
                                      +++*******++                                                  
                                         +++++++    
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_17():
    
    
        print("""                                                                                                      
                                                                                                    
                                                                                                    
                                                                                                    
                                          ++++++++                                                  
                                       +++*******+++                                                
                                     +++***********++  :                                            
                                    +***********+ +?S%?+                                            
                                   +************+:%@@#@#S*                                          
                                  +*************+%@%%#####?                                         
                                 ++  +*********+?#S:%#####@?                                        
                                +     +*********S##S######?S+                                       
                               +      +*******++##########*SS+                                      
                              ++      +*******  #########@*%@?                                      
                              +       +******* ?#########@?*#%                                      
                             +        +******+%#########?#S*+?*                                     
                            +         +*******S########S:##S %S                                     
                            +         +*****+*########SS ###+#S                                     
                           ++         +*****+?#?:?###S*#?+##*##*                                    
                           +         +******+%#:*S###%*##+S#*S@?                                    
                           +         +*****+*#%,S####S?@#*%#+S@?                                    
                          +          +*****+*#S*######SS#%##*#S%                                    
                          +          ******+?########@* ####S#+%                                    
                          +         +******+%#########:+SS?###?#*                                   
                         ++         *******+%@% ######%%*+*@###@?                                   
                         +         +******+*S@*:########% ######S                                   
                         +         *******+*## +@########%######S+                                  
                         +        +*******+*##?%###%%############+                                  
                        ++       +********+?#######*:S###########+                                  
                        ++       +********+?############S##%#####*                                  
                        ++      +*********+%############?##*#####*                                  
                        +*     +**********+%@###########*S#S%S###?                                  
                        +*+  ++***********+%?##########S S#@*%###?                                  
                        +**++**************%+S####?S###S+%#######?                                  
                        +****************+*S*%#### +@S*%*?#######?                                  
                        +****************+*#######S*##%?+*#######?                                  
                        +****************+*###S#######@S+*S######?                                  
                        +****************+*###+%#######%++S######%                                  
                       ++****************+*###*?#######%+*SS#####%                                  
                       ++****************+*###S%#######?+*??#####%                                  
                       ++****************+*############?+*%S#####?                                  
                       ++****************+*############?+*+S#####?                                  
                       ++****************+*############?+* +#?%##?                                  
                        +****************+*############? ?% #+%##?                                  
                        +****************+*###########@? *S?#%###*                                  
                        +****************+*###########S?+%#####?#+                                  
                        +****************+*###########??+%S####*S+                                  
                        +****************+*###########*%*SS#####?                                   
                        +****************+*S###S######*S?S#####@?:                                  
                        +****************+*S### %#####SS?##SSS##%                                   
                        +*****************+S###:%######S*SS*??##S+                                  
                        ++****************+%##S,%#######: *%SS##%                                   
                         +****************+%###?######S%  #####@%                                   
                         +****************+?#########?,+++#S####*                                   
                         +****************+?#########S+*?SS?+##%+                                   
                         +****************+*#########@*?#S?#+##S+                                   
                          +***************+*##############S%?##S                                    
                          +***************+*S#############** ?#%                                    
                          +****************+%#############%%*?#?                                    
                           +***************+?##############S?*#+                                    
                           +***************+?##S?######%###%?+S+                                    
                            +**************+*###*+S### *###%S+*                                     
                            +***************+S#@%:S### %###SS?+                                     
                            ++**************+%#######@+?@##S+?+                                     
                             +**************+*########+%SS#S??                                      
                              +**************+S#########%??SS+                                      
                              ++*************+%#########S%%S*                                       
                               +*************+*##########%*?                                        
                                +*************+?#####+S###?                                         
                                 +************+*S###S:%%@S+                                         
                                  +************+*####S%%S+                                          
                                   ++***********++?##?%*                                            
                                    ++************  *+                                              
                                      +++*******+++                                                 
                                         ++++++++  
                                                                            """)

