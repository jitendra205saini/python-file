def print_pattern_140():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                          ++++++ ++                                                 
                                     ++++++++++ ,+*++++                                             
                                   +++**++*??%%%?SSSS%?*++                                          
                                +++*+++*?S###############S?+                                        
                               ++**  ?%################SS##S?+                                      
                             ++*+++ +##################S??####*:                                    
                            +*++   :*########S*################                                     
                          ++++  +%?%#S#######S+S############S##+?%+                                 
                         +++   *S###SS######@* S###SS#####S*?##SS@S+                                
                        +++  :*SSSSSSS#######% ###S?: S###%%#######S +                              
                       ++   :+SSSSSSSS########+S## ?#?S######??S## ?+:                              
                      ++    +SSSSSSS#?+##########@*+@@#######?%##@S SS+                             
                      ++   *#SSSS#SS# *@##########%+S###########S%S##%%+                            
                     ++   +SSSSSS%SS# S??###########+?##########S  *%?*%                            
                    ++    ?%SSS#?+S##S#? ###########S:S@##########*? **#+                           
                   ++    ::?#SSS+%#####S:S###########+*S#####S S##%?#%%%S+                          
                   ++    :%#SSSS?S######?S############%%##### *####%#@**@%                          
                  ++   + ?#SSSS#S*#########S%################%#######@*%##*                         
                  ++   *SSSSSS#S: ?########S:%########################S###%                         
                 +*    S#SSSSS#++S+S#**#####? #############################*                        
                 ++   **%#SSSSSS##%S#%: S########?*%#######################*                        
                +*+   ?:SSSSSSS########?S####S*#*,**+?%S#################SS:                        
                +*   * *#S###S############### *?+:+++++*?S##############?*%**                       
                +*   %?S#S  +S###############+++++*****++*%#############S##S%:                      
               +++   S#SSS?*%#############%%?++  +++*****++%#?%#############S+                      
               +*+  +#SSSS###############%*         ++****++%S*##############*                      
               +**  ?#SSSSS############S*%?          ++****+*S#S+S###########?                      
               +**  %#S##SS##########S  S*+           +*****+?#%*############%                      
              ++**+ %#S%?S###########?*S#+             +*****+S#############?%+                     
              +***++?##?:+###?*?%#####@@?              +****+ *#############**+                     
              +***+*####S*S##S%?%#######?               +**+   SS##########%*?*                     
              +***+?%####@##############*               +*+    %#S#########?##*                     
              +***+???#################S++              +*     ?#SS###########*                     
              +***+%#?###########SS####S++              ++     *#SS###########*                     
              +***+%#############%*####S                ++     *?SSS##########*                     
              +***+%#############%*####S                ++      %#SS########S*+                     
              +***+%#############S?####S                +      *SSSS########SS*                     
              +***+%###############@?*@S+               +      :%SSS##########+                     
              +***+?################% ##+              ++     : +#?+##########+                     
              +***+?################# ##*              +      ++*# ?########?%+                     
              ++**+*##########*S########%             ++      ??SS*S#######S?%                      
               +**+*##########*S#########+            +      +S#SS##S#####S??+                      
               +**+*#S*######S+##########%           +       ?#SSSS#+?####%*#*                      
               +***+%S:#######S###########*        ++       *SSSSS##S ###@?*#+                      
               +***+?%:############S+%####S*++   +++       +S#SSSS####?*%#*S%                       
                +**+*SS############%,:?@###S*++++*+       +SSSSSS#####S?:%+%*                       
                +***+S@#######SS%%##S:*S####S*++**+      *%#*%?%#######+%#%?+                       
                +***+?#######?::,*@#@*:+####%*%++++    +:? ?*%?%###########*                        
                 +**+*########S%:S###S:%####?%##S%%*:+***SSSSSS###########*:                        
                 +***+%#######@%+#####+#####S*?####@S*?SSS################*+                        
                  +**+*#############################SS##%*################*                         
                  +***+%###########################%*?#@S:%#######%###?##%                          
                   +**+*S%#####################?#####@SS@??#######?%@??##+                          
                   +***+?*?S######@###########S+#####?*:*#########S?#?##?                           
                    +**+*?,+#####S?%##########S?##### %S?S*############?                            
                     +**+*%#####?::%###########S#####+%@S*%###########%                             
                     ++**+?######S+?##############+S#S#% %############+                             
                      ++**+?#####@++@############*?###@+,%###########*                              
                       +**++%#####%?####S########%S####*?:%#########*                               
                        ++*++%##########*%########++S###S+######?#%                                 
                         ++**+?#########S+###?%###*:+###?S#####%?#*                                 
                          ++**+?####%####%###S*S##S%%######S?##S?+                                  
                            +**+*%#S:#######%%?S##########%*S##?                                    
                             ++*++??*@##%###S?*S#%S##########%+                                     
                               ++*++?S##*S#######??########?+                                       
                                 ++*++*?*?#######SS####S??+                                         
                                   +++++:+?%SS####SSS?+ :                                           
