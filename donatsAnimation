def print_pattern_132():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                             +++++++                                                
                                         +++ :+++**+++++++                                          
                                     +++++**++%SSSSSS%%?**+++                                       
                                   +++*?%S###########@@##S%?*++                                     
                                 +++*%S############%*?S#####S:*++                                   
                               ++ *%################S%S######:+%?++                                 
                              +* *###SSS%%SS################# %##S*+                                
                            +++ +#SSSSS#?*#SSS#########%*S###%#####%*+                              
                           ++++*SSSSSSSS++SSS#SS######%*%#######%##?+++                             
                          +++%S#SSSSSSSS:?SSSS?: ?########S%#### +#% *?+                            
                         +++S#SS#SSSSSS#+*#SS:S#%%#######?*?S####+*@###?+                           
                        ++ %#SSSSSSSSSSSS%S## ?@@#############SS###@?%@#?+                          
                       ++ %#SS#%:S#SSSSSS###@?*S############## : ?*#? ###*                          
                      +++%#SSS#* SSSSSSS#######++##############S+S+ S+%##?:                         
                      ++%#SSSS#*??+#SSS######### *@########*S###*%@?S%*###S*                        
                     ++*#SS+?#SS#%:SSS##########%+*S######+ ####%%##@*+@###S+                       
                    ++,*##%+SSSSSS ?##############%S#####S #########@+?#####?                       
                    + +SS#%%#SSSSSSS#################################%S######*                      
                   ++ S#S#S+%#S##S####*+#####################################%+                     
                   ++%#SSS  +SS%S###### *####################################+ +                    
                  ++?#SS#%+S*%#*:?######S####S**S##########################%+**+                    
                  ++*#SSSS##SSS#% %#####@S+#@*:???+*?%S################S?*#**SS*                    
                 +* %###SSSSSSS########## ?#% :+++++++*?S##############%?S#?%@@?:                   
                 +++#%%%SSSSSS###########?%*+***++++***++?##S##################+                    
                 +*#S :*#SSSSS#######S%##?++++++   +++***+*S*S##################?                   
                ++?#SSS#SSSSS#######S*?#+ *++         ++**+*??##################%                   
                ++%#SSSSSSSS######S*%#@%+ +             +**+*#% ################S                   
                +*S##SSSSSSS####? *S?#S**+               +**+?*%#################                   
                ++#%SSSSS######S+?#@S**++                 +**+S@############@%*##?                  
               ++*#  S#%+*?#########??*++                 +**+*#SSS##########%:S@%                  
               ++%@#?S#S%?%#########S?++                   +*+ S#SSS########?**%@%                  
               ++*####SS############S**+                   +*  %#SSS#######%*####%                  
               +*+S###SSS##########S++++                   +*  *#SSSS############S                  
               ++%#########S?#######%++                    ++  *#SSSS############%                  
               ++S#########S*#######%++                    ++  +%SSSS############S+                 
               ++S#########S*#######%++                    ++   %#SSS#########S?#S+                 
               ++%##########%##?####%++                    +   *SSSSS#########*?#S                  
               ++%############# %###S+*+                   +   +SS#SS############?                  
               ++?############@+*@###**+                  ++  ::%S?SS############*                  
                +?######%%#####S%####?+++                 +    :S* #S############*                  
                +?######?%###########S+*+                +    ++S %#S########%?##*                  
                +**S####*%############*+*+              ++    %SS%S########@?*S##+                  
                +*:%####S#############S***+            ++    ?#SS#S%#######%%??#S                   
                ++ S##########*%#######%+**++         ++    *SSSSSS ?###### SSS#?                   
                 +*######@@##S::*#######?+**++++  ++++     +SSSSSS#% #@####:S@##*                   
                 +*S####????##S  ########%++****++++      *S#SSSSSS##???##%*####+                   
                 ++%####++:+##@?: ######S?%*+++++       +?S%SSSSSS###%%*+#+?@##*                    
                  +*####@#:%#### +#####S*%##%+        : +S*??%#SS####@* ?#S:##S:                    
                  ++%#####%#####+%#####%*?####S%? :+*+ %+??S??#S######S#@##S##%                     
                   +*####################%S######S++%SS###SSSSS#############+S+                     
                   ++*#######################SSSS%SSS%SSSSSSSS#############+*%                      
                    +++S#######################%*+%##*+SSSSS##############S%S+                      
                    ++:*##################*####SS#SS#S:%#S######S####S######*                       
                     ++%###*:+###########%*#####S?* ?#?S########*S##%*#####%                        
                      +*S##%?+###########S%#####S:#? SSS########%*#S+S####S+                        
                       +*##@#:S##############S##S #@#%+S#########%#S%###@#*                         
                       ++?###+%#############S+S##%#%**S#################%+                          
                        ++?#######SS#######S+S##### +##################?                            
                         ++?######S*########SS####%: +################S                             
                           +*######??###%####  S####S:%##############S*                             
                            +*S+%###S###??###*: ####*?########S#####?+                              
                             ++ S@#####S%*####%?####S########??##S ++                               
                               +*%#?S##%**S############S%%###*S@#%+                                 
                                ++?*?#######S?#########*?S#####%*                                   
                                  +++*S#####S*###########@###%*                                     
                                    + +?%S###S###########S%*+                                       
                                       +++*?%SSSSSS??%+**+                                          
                                            +++++++ + : 
                                                                            """)

