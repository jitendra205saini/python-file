def print_pattern_105():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                   +++++++                                          
                                                ++++*****++++                                       
                                              +++++**********++                                     
                                           +**SSS%*++**********++                                   
                                          ?S# %@#@#%*+***********+                                  
                                         %@?+?%##%#@%++***********+                                 
                                       +*S#**##S%+###%++***********++                               
                                      +S%S#%S##*:*####% ************++                              
                                      S#%SSS+S#S%SS####*+************+                              
                                     ?@S*SSS +SS#SS####S +************+                             
                                    *##%%?S#S% %#SSS++##?+*************+                            
                                    ?###??SSS# ?#SSS??###++************++                           
                                    ? SSS#+::?*?#SSS#####?+*************+                           
                                    *S##S#%*?++SSSSSS####S+**************+                          
                                  +?*S###SS*S%:SSSSSS#####*+*************+                          
                                  ?#?*###?S?S?+#SSSSS#####%+**************+                         
                                  S##?*@#+S##%?#SSSSSS#####*+*************+                         
                                  S###+?#+##SSSSSS#SS %####?+**************+                        
                                  S####S####SSSSSS%SS:?####%+**************+                        
                                + S##########SS##%:SS:%####S*+*************+                        
                                +?S##########S#%%?:SS?S#####*+*************++                       
                                +##*%###S#####%+S**#S##? ###?+**************+                       
                                +#@%?###?*##%#%%#%SSSS# +###%+**************+                       
                                ++S###?%#:S?+S#SS#SSSS#%####S+**************++                      
                                %%S### %#%%++%#SSSSSSS#######*+**************+                      
                                S@####S###*++*#SSSSSSS#######*+**************+                      
                                S###S%##S***+*SSSSSSSS######S*+**************+                      
                               +###S%S##% ***+%#SSS#S####### :+**************+                      
                               +##%*%@###*+**+?#SSS?S########?+**************+                      
                               +#S+SSS?#S****+?#S#%:S########%+***************+                     
                               +S##S#?%@% ***+*#SS%:%########S+***************+                     
                               *###SSS#@? +**+*##** ?########S+***************+                     
                               *###SS%%@?  +*+*SS+SS#########S*+**************+                     
                               *S#SSS%?#*   +**SS%S##########S*+**************+                     
                               *?##SSSS#*   +*+S#SS###########*+**************+                     
                               *%SSSSSS#*    ++S#S###########S +**************+                     
                               *#%*#SSS#*    ++S#############S,+**************+                     
                               *#%+#SSS#?     +S#############S:***************+                     
                               +##?SSSS#?     +S###%+########S ***************+                     
                               +###SSSS#%      S##S*%#########*+**************+                     
                               +##SSSSS#%      S#############S*+**************+                     
                               +S?#SSSSSS      S#########??##S*+**************+                     
                                ? SSSSSS#+     ##########*%##S+***************+                     
                                *+%#SSSS#?    +##########+S##S+**************++                     
                                ?**#SS#S#%    *###**#####S###S+**************+                      
                                ??S#SS?SSS*  :*@#S*##########%+**************+                      
                                *##SS*%SS#?: :?@S???S########?+**************+                      
                                +###S?%#S#S+  %@?+S*%########?+**************+                      
                                 S##SSSSS#?* +?#% @##########*+*************++                      
                                 %##SSSS##***%*S?+###########*+*************+                       
                                 *##?%#S?%SS%#*+*?@#########S+**************+                       
                                  ##?S##? %###+%% ##########%+**************+                       
                                 +S###%?%:S####@S:S####S%###?+*************+                        
                                  ?###*:*?#######S#####%,?@#*+*************+                        
                                  +#%%?%?###############%%#S*+*************+                        
                                  +S%#S%?##?S#####S ?######%+*************+                         
                                   +%S% S##?S#####+ #######?+*************+                         
                                   +****@##%%%%###S########*+************+                          
                                    *+S+####S*S#########%#%+*************+                          
                                    +%#*#####S#########S+%*+************+                           
                                     ?#S#################%+************++                           
                                     +S@#################*+************+                            
                                      *#################S+************+                             
                                       ?S*#%#######% ###*+***********+                              
                                        *S#?%###### *@#?+***********+                               
                                         ?#SS#S*###S##%+***********+                                
                                          *%#@# %@#%%*+**********++                                 
                                            *%#S##S*+++*********++                                  
                                              +*??************++                                    
                                                 +++******++++                                      
                                                    +++++++   
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_106():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                   +++++++                                          
                                                ++++*****++++                                       
                                              +++++**********+++                                    
                                           +*??SSS?*++*********++                                   
                                          ?S#%:###@#?*+**********++                                 
                                         %## ??S##%##%++**********++                                
