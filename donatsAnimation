def print_pattern_130():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                            +  ++++++                                               
                                         ++ :++++**+++++++                                          
                                      ++++++ ?SSSSSS%%%?**++++                                      
                                    ++**%S##########@@###S%?++++                                    
                                 +++*?S###########%*?S######  ?*++                                  
                                 +*%S#####S########S%S######  %S?*++                                
                              ++ ?###SS%SSSSSSS#############+%###S?++                               
                             ++ ?##SSS#*?#SSS##S######?*####S######%*++                             
                            ++ ?#SSSS#%*+SSS#%S#S####?*S#######?S@?+ *++                            
                           +*%S#SSSSS#%:%SSSS+: %#######S%?S###*:%#%*S%++                           
                          +*S##SSSSSSSS ?##% S%?%#######??%####@%+#####%++                          
                         ++S#SSSSSSSSS#?%#SS:%###############?%S##@% ###%*+                         
                        ++%#SSS+SSSSSSS##SS#+?S##############+ :% %# ?###+ +                        
                       +*%#SS#?:S#SSSSSSSS#### *#############@#+S?:?%*###%+++                       
                      ++%#SSS#**??#SSSSS######S:?@########+S###*?@SSS #####?+                       
                     ++?##?*#SSS?:SSSSS#######@?+*S###### +####S%##@% ######*+                      
                      :?#S+%#S##S:%#S###########S%S#####S+#########@?+######S++                     
                    + +S#S?#SSSSS%SS#########################################%+                     
                    + S#S#?%#SSSS#S##*?######################################?*+                    
                   ++%#S#?:+SSSS#S###S:?###################################SS *+                    
                   +?#SSS %*?#*:?#####S%#####*+%#########################S#+?*%*+                   
                  ++?#SSSS#SSSS*:%#######*S@%:*??*+?%S#################?+%# SS#%+                   
                  + %##SSSSSSSS#########+*#S*:++++++++*%##############S?S##?##@?:+                  
                 +++#%SSSSSSSSS#########?S?++***++++**++*%#S###################*++                  
                 +*S?: SSSSSSS#######%##S*+**+++   ++++*++?%?###################S+                  
                 +?#SSSSSSSSS#######**#%:+*++         ++**+?*####################*+                 
                 +%##SSSSSSSS#####*?###* ++             +**+?S %#################*                  
                 +S@#SSSSSSS###% *S?##?+*+               +***?*##############S###*:                 
                 +SS#SSS##SS##S+?S##*?**+                 +*+*##SSS#########@+?##S+                 
                +*%:*#S**?S#####@##%??+*+                  +*+%#SSSS#########* ###*                 
                +?@S%SS%%?S########SS**+                   +*++#SSSS#######S+%+S##*                 
                +*###SS#############%+*+                   +*+ SSSSS#######*?#####*                 
                ++S###SSSS#########*+**+                    ++ %#SSSS#############*                 
                +?#####SS##?########*+++                    ++ ?#SSSS#############*+                
                +%#########+########*+++                    +  *SSSSS#############*+                
                +%#########*S#######*+++                    +  +SSSSS#########S%##?+                
                +%#########S##*S####*+*+                    +  ?#SSSS########S+?##*                 
                +?############ ?@###?+*+                   +   ?##SSS########SS##S+                 
                +?###########@?+####%+*+                   +  :+#%SSS############?+                 
                +*#####S*######%####S*+*+                 +   :+S %#S############?+                 
                 *%####%?############?+*+                 +    %* #SS########%S##%+                 
                 + ####%%############S++*+               +   +SS%%#S########**S##%+                 
                 +:S#########S########?+**+             +    %#S##SS#######S?%%##*                  
                 +*##########+*########*+**++         ++    ?#SSS#+*######%+S??##+                  
                  *########## ,+########*+***++++ ++++     *SSSSS#%:%@###@?+@###%+                  
                  +S###*   ###* %######@#?++****++++      ?SSSSSSS#S%%%###?*####?+                  
                  +?###S% +###%: ######S?S%* ++         +%%SSSSSSS#@%***SS ####S*                   
                   +S###@+%#### *@####S*%##S*           %%*%?#SSS####S :%# ?###  +                  
                   +?##########?S#####S**S####%?*:++*:?*+%%%*#SS#####%?###%?@##+                    
                    +S##################SSSSSS###*+?SS##SSSSSSS#############%#%+                    
                    ++S###################SSSSSS%SSS%SSSSSSSSS#############++#*                     
                    +  S####@############SSSS#%*+?##?+#SSSSSS#############?*#?+                     
                      +S##S??###########S*##SSSS#SS#S:%#SSS################@%++                     
                      +?##* +###########%?####SS?* %#?%#S######*S###?S######*                       
                       +%### S##########SS#####% S* SSS########%*##?*######?+                       
                        *S## ?@#############S#@%+#@#%+S#########?##*######%+                        
                        +*S@%S#############?*###S@%?*S###################%+                         
                         +*S@####%%#######S*######:*###################S++                          
                           *S#####*S#######S%####?::?#################S                             
                            *SS%##%%##S?###S: S##S#%:S################?                             
                             + *@######??###*:+####+%###############S*+                             
                               *S#S###??*####S%####S########S?###*%%++                              
                               ++%??@#S?*S#############S%###?%###*:+                                
                                 ++*%######%?#########**%###S##S?++                                 
                                   + *S####S*################S?+                                    
                                      +*%S############@##S%?*+                                      
                                         +*?%%%SS#%?S???*++                                         
                                             ++++++++:                                              
                                                       
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_131():
    
             print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                             + ++++++                                               
                                         +++: +++**+++++++                                          
                                      ++++*+ *%SSSSS%%%?**++++                                      
                                   +++*?%S##########@@###S%?*++                                     
                                 +++*?S############*?%#####@?:*+++                                  
                               + +*%###############SS%#####@?:?S?++                                 
                              +* ?###SSS%SSSSS##############?+###S?++                               
                             ++ *#SSSSSS+SSSSS########S*?####S#####%*+                              
                           +++ ?SSSSSS#?+?#S#S%S#####S*?#######S%@S* ++                             
                          ++*%S#SSSSSS#++SSSS% :+########S%%###S:*#%+%?+                            
                         +++S#SSSSSSSS#%:SS#+*#%?#######S*?%###@S %@###%+                           
                        ++ S#SSSSSSSSSSS?SS#? #@#############S%S####+S##%++                         
                       +++%#SS#**#SSSSSS#SS#S SS#############? :**?#++###*                          
                       ++%#SSSS:?#SSSSSSS#####%:S##############??S +S+###? +                        
                      ++%#SSS#% S+SSSSSS#######?:########@?*###S+##%#*%####?+                       
                     ++?#SS+SSSSS ?#SS##########+*?######%:%####?S###:%####S*                       
                    ++:?##*?#SSS#* #S############S%######*?@######### S#####%+                      
                    + +SSS?S#SSSSS%##################################S#######?+                     
                   ++ S#S#%*SSS#SS###S S#####################################%+                     
                   +*%#S#%: %#S%######*:S####################################+++                    
                   +%#SS#**%*#S:+S#####%#####%+?#########################S#?*??*                    
                  ++?#SSSS#SSSSS++#######??@#  ??*+??S#################%**#+?SS?+                   
                  + %##SSSSS#SS#########?:#S?: *++++++*?S##############?%##?S#@?:                   
                 +++#%%SSSSSSS##########%%%*+**+++++**++*%#SS##################*++                  
                 +*#%: %#SSSSS#######SS##?+**+++   ++++*++?S*###################%+                  
                 +?##SSSSSSSS#######?+%# +*++         ++**+**%##################S+                  
                ++%##SSSSSSSS#####?*S##? ++             +**+*#+*#################+                  
                +*S@#SSSSSSS###S* S%S#%+*+               +**+?*S#################+                  
                ++S%#SSSS#SS###?+%#@?***+                 +*+*###S##########@?*@#%+                 
                +*S:+SS?+*%########S*?+*+                 ++*+?#SSS##########?:S#S+                 
                +?@#?SSS%?S#########S*++                   +*++SSSSS########*?*%@S+                 
                +*###SSS############S+*+                   +*+ %#SSS#######?*####S+                 
                ++S###SSS##########?++*+                   ++  ?#SSSS#############+                 
                +%#########?S#######?++                     +  *#SSSS############S+                 
               ++S#########?%#######?++                     +  +SSSSS#############*                 
                +S#########??#######?++                    ++   S#SSS##########?##*                 
                +%#########SS#%?####?+++                   ++  *#SSSS########S*?##+                 
                +%############% ####%+*+                   +   +#S#SS#########S##%+                 
                +?#############:S###S+*+                   +  : SS?#S############*+                 
                +?######*S#####%#####*+++                 +   : S+*#S############?+                 
                +*S#####*############%+*+                ++    *%:SSS########%%##%                  
                +*+####S*#############*+*+               +   +SSS%##S#######?*S##?                  
                 +:S##################%+**+            ++    ?#SS#SS#######S%??##+                  
                 ++##########%+S#######?+**++         ++    *#SSS#%:S#####S S%%#S+                  
                 +*#####@@###*,:S#######?+**++++  ++++     *SSSSSS#*+@####S:####?                   
                 ++S###%+*+%##%:?########?++****++++      *S#SSSSSS#%%%%##%+####*                   
                  +%###S**,S### :?@#####?%%*++++        +%%SSSSSSS###??*?# S###%+                   
                  +*####@?+####%:S#####%*S##?           ?S+%*SSSS#####+:?@* ##S:                    
                   +%##########%*######?*%####S??: +? *?+%?S*SSS######%####?##S+                    
                   +*###################S%###S###?+*SSS##SSSSSS#############?#?                     
                    ++S#####################SSSS%%SSS%#SSSSSSS#############++S+                     
                    +++S####################SSS*+*S#S %#SSSSS#############%?#*                      
                       %###S%S###########?%##SSSSSS##*+#SSS#####S##########@?++                     
                      +%##%::%###########*S#####?% *SS?########%*###??#####S+                       
                      ++S##S*?###########%###### ?%+*#S#########*S@%+S#####*                        
                       +*##@? ###############S##+%@@#**#########%S#%S#####?                         
                        +*##S*##############*?##%##***##################S?+                         
                         +*#######%########*?####@?,S##################%                            
                          +*S#####??#######SSS@###   %################S                             
                           +*S#%###*###%S##@?:?###S#++################?                             
                            +*? #######S*S##% :%##@%+S#######S######%*                              
                             + +S#####S?*%###S%S###SS########*S##?+?++                              
                               +*%S*###?*?#############S%S##S*###%                                  
                                ++**%#######*S########%*?S#####S*++                                 
                                   + *S#####?%###############%*+                                    
                                      +?%S############@##S%?*+                                      
                                        ++*?%%SSSSS?%%*?*+                                          
