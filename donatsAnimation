def print_pattern_112():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                +++++++++++++                                       
                                              ++***++++******+++                                    
                                           +*?%S#S*S%%*+++******++                                  
                                        : ?S####S%:%@##S?*++******++                                
                                       +?SSS#SS#*+%%#####S?++*******+                               
                                      *S#S**SSS#**######*%@S*+*******++                             
                                     ?##SSS#SSSSSSS#SS+: S###?+*******++                            
                                   +%##SSS#?*SSS#S ?#SS%S#####?+*******++                           
                                   %#SSSS#%+SSSSS#?,%SS######@#?+********+                          
                                  ?*#S##SSSS%+?####%S? SS###S?%#?+********+                         
                                 ?%*#% ?#SS#??SS%%%%#%:%#S##S :S#*+*******++                        
                                +# %%???#SSS###%::*:*S+?#S#######S*+*******+                        
                               +S#+%%+##SSSSSS#SS+S% S+%#SS#######%+********+                       
                               ?##%SS %SSSSSSSSS#*?##%:S#SS########*+********+                      
                               S#S#SS%+*#SSS#%+##%%##? SSSSS#######S+********+                      
                             + %#SSSS#S ?SSSS *#SSSS#?*#SSSS########?+********+                     
                             ++?#SSSSS#%*?###*SSSSSSSSSSSSSS###S*###S*+*******++                    
                             ?% SSSSSS##########SSSSSSSSSSSSS##%:####?+********+                    
                             %#+S##SS############SSSSSSSS#S#+?@%:####S+********+                    
                            +##SS%?S##############SSSSS#S?SS:%@S*#####*+********+                   
                            +##SSS ?####S%#########SSSS%+*S%:######*%@%+********+                   
                             %?SS#SS####%:S#%**%####SSS?%S#%?#####?,S#S*+********+                  
                           +?S*+#S###?*@#:?*++++*%##SSS##SSS######?%###?+********+                  
                           ?@#SSSS###+%##?++****++%##SSSSSSSS##########%+********+                  
                           %##SSSS###S#@S*+*******+%#SSSSSSSS##########S+********++                 
                           %##SSS#S*###*++*******+ +##SSSSSSS##########S++********+                 
                          +###S#S?%S###++*******++ :?#SSSSSSS#@########+, ********+                 
                          *###S%?*S@###*+*******+   +#SSSSSSSS?########S*+********+                 
                          *@##S ?##%?@%+********+    S#SSSSS#S,S#######@%+********+                 
                          +?##SS#S#*S#?+*******+     ?#SSSSSSS +########S+*********+                
                          ?S##SSSS#S##*+*******+     *#SSSSS+?% ########S*+********+                
                          %@##SSSSS%#S+********+     +#SSS#**###########S*+********+                
                          %@SSSSSSS?%S ********+     +S#SSS%S############*+********+                
                          %@?%#SSSS##% +*******+      S#SSS##############*+********+                
                          %@?%##SSS##%  *******+      %#SSSS#############:+********+                
                          %##S?%#SSS#S  +******+      %#SSS##############:+********+                
                          %###?+#SSS#S   +*****+      %#SSS#####S######## +********+                
                          ?%##%+#SSS##   +*****+      %#SSS###%+*########++********+                
                          *?##SSSSSSS#+   +****+     +SSSS####*%#########*+********+                
                          +%##SSSSSSS#*    +****+     S#SS###########SS##*+********+                
                          +###SSSSSSS#%    +****+     SSSS###########+?@S*+*******++                
                          +#@?+SSSSSSSS+    +***+     SSS###########S+##S+********+                 
                           +%S:%#SSSSS#?     +***+   %#SS#####%#####S?##%+********+                 
                            *@++SSSSSSSS+     +***+ +SSS####%+*#########%+********+                 
                           +%@? #SSSSSS#%      +**++++#S####??##########?+********+                 
                           +##%%#SSS%*SS#?       ++?%:S####+S*+?########*+*******++                 
                            S###SSSS+%#SS#+       +S#*S###S:S#%%#######S+********+                  
                            *@###SSS%?SSSSS?:  : +S##%*%##S:S##########%+********+                  
                            +S###SSS##SSSS#?:?+: *SS#%?*S#?+###########?+********+                  
                             +####SSSSSSSSSS?SSS%?SSS#% :#+*@##########*+*******+                   
                              S####%*SS#%+?S?#SSSSSSS#?*S#%:S######S##S+********+                   
                              ?@###%%#SS#S#? SSSSSSS#######+%#####?,*#?+*******+                    
                              *#####SS#S*+%% #SSSSS################%?S*********+                    
                              +SS####SS% * S%SSSS############%?#####@?+*******+                     
                               *?###S*S%*#%?SSSS#%S#########?,*######*+*******+                     
                                +S##%S##S?**SS###%?###S#####*%######%+*******+                      
                                 #%S#?##% %######S*##*%###########%S*+******++                      
                                 ?S?# ?@*:S#######%#%+###########%++********+                       
                                 +*+#* ##?+#########SS############%+*******+                        
                                  +*S#%##??######################S*+******+                         
                                   +SS###%######################S*+******++                         
                                    +??#####################*%@S*+******++                          
                                      *#####SS##SS#########*:#S*+******+                            
                                       *###S**##??#########?SS*+******+                             
                                        +%######%%###+?####S?++*****++                              
                                          +%S########% ##?++++****++                                
                                            +*%S######S%?*+++***+++                                 
                                               :+***?**++***+++++                                   
                                                   ++++++++++
                                                                            """)

