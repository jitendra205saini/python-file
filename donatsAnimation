def clear_screen():
    os.system('cls' if os.name == 'nt' else 'clear')

def print_pattern_1():
    
             print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                            +                                                       
                                      +*********: ***                                               
                                    *********%###?####+++                                           
                                 ********################S+                                         
                               +****,*?#####################+                                       
                              *******:##################*+?###++                                    
                            ***     ,##########+###############,                                    
                           **    *#SS########## ###############++                                   
                          *+    #SSSSS########+:%###########+#####?                                 
                         *    ,#SSSSSS#########,####+, ####+#######%                                
                        *    :SSSSSSS##########,###,##+########S##,#,                               
                       *     SSSSSSSS##,########### ?########**S###,#*                              
                      *     SSSSSSSS##%,############:#################+                             
                     ++    +SSSSSS*S##:%#,###########,###########,,##:#                             
                     *     S%SSSSS+######,############,###########+#,#:#+                           
                    *     :,SSSSS+*######,############ ?######%+###*# ##+                           
                   *+    ?,SSSSSS*#####################*######,####+##:##                           
                   *     ,SSSSSSSS,?##################################:##?                          
                  **    %SSSSSSSS:,+#########,############################                          
                  *     SSSSSSSS#,#+%#S,######,###########################?                         
                 +*    ?:SSSSSSS####S## ,#########++#######################,                        
                 *+    S,SSSSSS################:#,,*:*%################### ,                        
                +*    +,?SSSSSS###############,?*, ******###############++ ?                        
                **    #SSSS,,,S###############************S###############S#,                       
                **    SSSSSSS##############%#+     ********?#S%#############+                       
                **    SSSSSSS#############**,       +*******##+#############%                       
               +**   ?SSSSSS#############+? +        +*******###,############                       
               ***+  #SSSSS############:,#*           ********## ############                       
               **** +?SSS?#############*##+            *******##############                        
               ****  +#SS,,####++*#######*             ******+?#############,                       
               ***** ######S#####*#######              +***    SS##########                         
               ******?###################               **     SSS#########%#+                      
               *****?*+##################+              *      SSSS##########+                      
               *****S#+############%#####               *      SSSS##########*                      
              +*****###############+####S               *      S*SS##########?                      
              ******###############+####%               *       SSSS########++                      
               *****###############+#####                      SSSSS#########                       
               *****S#################,##                      ,SSSS#########+                      
               *****?#################,##              *      :? S?*#########                       
               ******#################,##                     *:SS,#########+                       
               ******###########+########+            *       S%SS*########+#                       
               ******##########?*#########                    SSSSS####### ++                       
               ******##,#######+%#########                   SSSSSS#,S####,#*                       
                *****##,#######S###########         +       *SSSSS###,####,#                        
                *****?#,#############+%####?+     *         SSSSS##### +S#S#                        
                ******# #############,:+####*******        SSSSSS######+,#,%                        
                +*****################ ,#####%*****      +SSS*S?####### ###                         
                 *****S########,,,,####,,#### + +***    ,S,,*SS*##########+                         
                 ******##########: ####+ ####?####??,,%*:SSSS#############                          
                  *****##########,#####,######+*#####  #SSS###############+                         
                  ******#################################+###############*                          
                   *****############################++%##*,####### ###+##                           
                   ******####################### #########,########*# ##+                           
                    *****#+%####################+#####,,,##########+#%##                            
                    ******#,,###################+#####,###+*###########+                            
                     *******######,,:#################,###+###########,                             
                      *****#######%#:############## #### *###########S                              
                       *****########,############# S####,,##########S                               
                        *****#######,#############*#####:S,########S                                
                        +*****###########+#########,:#### %#####+#S,                                
                          *****###########+###*#### :*###*######+%                                  
                           *****##########?####+####%%###### ###                                    
                            *****?###,#########%###########+###                                     
                              *****S#,###S####++##+##########+                                      
                               +*****####+########+########+                                        
                                 +****+?#+########S#####%                                           
                                    **** ,+##########% +:                                           
                                       +**********+   
      
                                                                          """)

print("\n\n\nrunning animation...")

def print_pattern_2():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                        +++++++++ +                                                 
                                     ++++**+++++ ,++++                                              
                                  +++***++*??%S%S?SSS%?*+                                           
                                ++**++++*%S#############S%*                                         
                              ++****+:?S################S##S*                                       
                             ++*++++* ?@#######S########%*?##S+                                     
                           ++*+    ? +S########??#########S##@#                                     
                          +++    +S#%##########?+#############@? +                                  
                         +++    *S#SS########## +####S#####S*%@S%#*                                 
                        +++   : S#SSS#########@?+###%  %###%%######*                                
                       ++      S#SSSS###%#######+##S #??######%%##*?                                
                      ++     +S#SSSS###%:########### S@######%*?##%+?+                              
                      +     +S#SSSS###@*+###########+?#############%#%+                             
                     ++     %#SSSSS?S##*?%+##########?+##########+ ?S+*                             
                    ++      +?#SS#%+#####S:########### ?@#########** *?*                            
                   ++     +: %#SS#*?###### %##########?+S#####%*##%?%??%                            
                   ++     + S#SSSS%#######%S###########%?####S:%###?##+S%                           
                  ++     *+%#SSSS##+?########SS##############S%#######+S#+                          
                  ++     SSSSSSS##+:+S#######S:S######################%##?                          
                 +++    *SSSSSSS#S S??@% S####*+#########################S+                         
                 ++     %+%#SSS######S##+:%#######%*%####################@+                         
                 ++    *?:SSSSS##########%S####*S*:***%S#################S                          
                +*+    % ?#SSSS###############* ? :++++*?S#############S*?++                        
                +*+   +S%SS#*::%##############*+++*****++*%############SS#%*                        
                +*    *##SSS%%%############S?*+  +++*****++%@%%###########@%                        
               ++*+   ?#SSSS###############?       ++*****++SS*#############+                       
               +**+   %#SSSS#############S*?        ++*****+*#SS+S##########+                       
               +**+  +SSSS#############S+ %+         +******+%@%*###########*                       
               +**+   %#SS?S###########%*S%           +*******#############?*                       
              ++***+ +%### :%###**?#####@#*           +****+++%############* +                      
              ++****+%#####??###S%?######S+           ++*++   *###########S**                       
              +*****+S%S#################%             +*+    +SS#########%SS                       
              +*****+S?*#################?+            ++      S#S##########S                       
              +*****+S#*############%####*+            ++      %#S##########S+                      
              +****+*S##############*S###*             ++      *%#S#########%+                      
              +****+*###############*S###*             +       *SSS########S*                       
              +****+*###############*S###*             +       ?#SS#########%                       
              +****+*S################%*@*            ++      :+S#SS#######@%                       
              +*****+S################S #?            +         *#+?########?                       
              ++****+%################# SS            +       ? %S:S#######?+                       
               +****+%##########S?#####S##+          +       +S%SS%#######S?*                       
               +****+?##########%*########?          +       *##SS##%#####*?+                       
               +****+?##?#######??########S         +        SSSSS## ?@##S+S                        
               +****+*##:S######%S#########?       +        ?#SSS###S*#S#S*%                        
               ++****+SS:S############?%####*    ++        *##SSS#####**?*%*                        
                +****+%#+############# :?###S*+++*+       *SSSSS######S *%+                         
                +****+*############S##%  ####S*++*+      *%S?*%?######S%##?                         
                +****+*#########+  :##@?:+####**++++   * *++%%S?#########S                          
                 +****+%########?? +###S:*###S?##S%%*:***S#S#############*                          
                 +****+?########@# ?####+%####?*####@%*%#S###############*                          
                  +****+S#########S#####S############S%##*?###########S#S+                          
                  +****+*###########################%*?#@S:S######?%@%?@?                           
                   +****+S######################S%####@S%@?%######S*#*#S+                           
                   +****+?S*S@##################??####?+ *#S#######%#S#*                            
                    +****+%% *#######S##########%%###@+?#S%*##########*                             
                     +***+*% *#####*:+###############@??@%+S#########%                              
                     ++***+*#@####S*+*@############?*###S:S##########+                              
                      +****+?######@? ############S+S##@*,*#########*                               
                       +****+%######% ############S%####?% %####S#S*                                
                        +***+*%######S####?S######@%:?##@%*#####?#*:                                
                         +***++%##########%*###?S##S :S##%S####S**                                  
                          ++**++?##########?##@%*###S%#####S*%@S+                                   
                           ++***+?S##S #######S%*##########%S#%+                                    
                             ++**+*%#? ###S###%*?##?########S*                                      
                              ++**++*?S@@S*#######S*######S*                                        
                                ++**++*?SS*########%###%**+                                         
                                  +++*+++ +*SS#####S%%* :                                           
                                     ++++++++*****++                                                
                                          +++++      
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_3():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                        +++++++++                                                   
                                     ++++**+++++ : +++                                              
                                  +++***+++*?%SSS%%SS%?*+                                           
                                ++***+++*?S#############S%*                                         
                              ++***** +%###################%+                                       
                             ++*++++** %########S#######S*?##%+                                     
                           ++*+    *% ?#########*%########S##@S                                     
                          +++     ?#SS##########**############@? +                                  
                         +++     ?#SS###########:*####S#####%?#S?%+                                 
                        ++     :*#SSS###########+*@#S?:*###S*S####S++                               
                       ++     :+SSSSS####%######%*#@**S*S#####SS##?*                                
                      ++      ?SSSSS####?:##########% @@#####S*?##?++                               
                      ++     *#SSSSS###@++##########S S###########@?#?                              
                     ++     +S#SSSS#?S##*%%+##########+%#########++%S++                             
                    ++      + *#SS#%+#####S:#########@? #########S*+++*                             
                    +      *:+SSSS#*?###### %#########S ?#####S*#@%??*%*                            
                   ++     +++##SSS#SS######%S##########S?S#### ?###?##+S+                           
                   +      ?*%#SSS###+*########SS##############?#######+#?                           
                  ++     +S#SSSSS##+:+S#######% ######################%#S+                          
                  ++     ?SSSSSS##S S%?@% S####+?########################?                          
                 ++     +S %#SSS######S##*:%######S**####################S                          
                 ++     ?* SSSS###########%S###S*%: *+%S#################%:+                        
                +*+     % ?#SSS################ +*:+*++*?S#############S** +                        
                +*+    +S%SS#?::?############@S+++*****++*%############SSS?                         
                +*+    *##SSSS%%############S?+  ++******++%@%%############                         
                +*+    %#SSSS##############S*:     +******+*SS*############*                        
               +**+    SSSSS##############%**       +******+*#SS+##########?                        
               +**+   +SSSS#############S  ?         +******+%@?*##########%                        
               +***+  +%SS#?S###########%*S*         +******+*#############*+                       
               +***+  *%###+,?###**?S####@S+          +****+++S############                         
               +****++%#####%?###S%?S#####%           +**+    ?###########?++                       
               +******S%S#################*           +*+     *#S#########%#+                       
              ++****+*#?*#################*+          +*+     +SSS##########+                       
              ++****+*##*############S####++          ++      +SSS##########*                       
              +*****+*###############*S##S+           ++      +*%S##########+                       
              +*****+*###############*S##S+           ++       *SS#########?+                       
              ++****+*###############?S##S+           ++      +%#S#########S+                       
              ++****+*#################%*#+           +       :+S#%S########+                       
               +****+*S################S #+           +         ?# %#######S+                       
               +*****+S################# S?          +        % S% #######S*                        
               +*****+S###########?#####S#%          +       +S%SSS#######%*                        
               +*****+%##########S*########+        +        ?##SS##?####%**                        
               +*****+%##%#######%*########?       ++       +SSSS### ?@#@??*                        
               +*****+?##:S######S%########S+     ++        %#SSS###S?SS#?%+                        
                +****+*#S:S############?%###%    ++        ?##SS#####S?*?*?                         
                +*****+S# S############ :?###?+++*+       ?#S%SS######% %*                          
                +*****+?##########@####?: ####%+++*+     *%S%*%?######SS@S+                         
                 +****+*#########?++:S#@?:+###%++++++  *++*+%%S?#########*                          
                 +****+*S########** :###S:*###%%#S%%* +**S##############S                           
                 +*****+%#########@+*@###+%###S*?###@S?%################S+                          
                  +****+*##########%#####S###########S%S@%+#######S###S#*                           
                  +*****+%###########################??S@# ?@#####?%@?SS+                           
                   +****+*#######################%S#####?#S%######S?S?#*                            
                   +*****+%S?#@##################*%###S++ SS#######S##%                             
                    +****+*S?+%##################?S###S #SS+S########S                              
                     +****+?%:+#####? +##########S#####*#S*%#########*                              
                     ++****+%#######+ *#############*S### ?@########%                               
                      +****+*S######@? ############?*###S: %#######%                                
                       +****+*#######%:############%S###S%**####S#%                                 
                        +****+?#######%####?S######@* %###*S####?S+                                 
                         +****+*S##########?*###?###?:+##SS##S##*+                                  
                          ++***+*S##########?###*?##S%S####S*?#%+                                   
                           ++***+*%###S*#######S%?#########SS#?                                     
                            ++***++?S@*+#######?*%@%S#######%+                                      
                              ++***+*??S@#%?#######??#####S*                                        
                                ++***+*?S#%*#######SS@#S*+                                          
                                  +++**++ ++%######SS?*+                                            
                                     ++++++++*****++                                                
                                         ++++++   
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_4():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                       +++++++++++                                                  
                                    ++++****+++*+ :+++                                              
                                  +++***+++*?%%SSS?SS%?*+                                           
                                ++***+ +*?%#############S?+                                         
                              ++*****+:?S##################%+                                       
                             ++*+++*+** S########S#######?*S#?+                                     
                           ++*+    +%? %#########+S#######SS#@%:                                    
                          +++     +%#############+%###########@?:                                   
                         +++     +S#S###########S:%###S#####S?#S*?                                  
                        ++      :%#SS############ %##S+ %###*%####?                                 
                       ++      :?#SSS#####?######??## %?*######S##?+                                
                       +       %#SSS#####?:##########+?@######**S#*+                                
                      ++      %#SSSS####@*+##########?*############?S+                              
                     ++      *SSSSS##?S##?%?+#########%+#########*+SS                               
                    ++      ++:*#SS#S+S####S:########## ?@#######S+++++                             
                    ++      ?:*SSSS#?*###### %#########*+######*#@%?**?                             
                   ++      *++#SSS##SS######%S##########?%####*+##S?#S*?                            
                   ++      %*S#SSS###++########%##############*S#####S+#+                           
                  ++      +##SSSS###+:+%######@*+####################S%@?                           
                  ++      %SSSSS####+SS*#% S#### S######################S+                          
                 ++      +S %#SS#######S##*:%######?*S@##################+                          
                 ++      ??:SSS############%S###??+:*+%S#################+                          
                 ++      S ?#SSS###############%:*  *++*?S#############%?                           
                +*+     *#SSS#%::+#############*+******++*S############%S%+                         
                +*+     ?##SS##SS############%* +++******+*S@%%###########*                         
                +*+     %#SSS###############S+     +******+*#S*###########%                         
                +*+    +SSSS###############%*+     ++******+?#SS+#########S+                        
               +***    +SSS##############S  *       +********S@?*##########+                        
               +***+    %###%%###########%?S+        +******+?############?                         
               +****+  *?###?,+###?*?S####@%         +****+++*###########@?                         
               +*****++S#####S*###S%?S#####*         +**++    %##########S*+                        
               +*******SSS####@############+          +*      ?#S########SS?                        
               +*****+*#?*################S+          ++      *#S#########@?                        
               +*****+*##*S###########S###S++         ++      *SSS#########?                        
               +*****+*###############*%##%           +       +*SS#########?                        
               +*****+*###############?%##%           +       +?SS########S+                        
               +*****+*###############?%##%          ++       +S#S#########?                        
               +*****+*#################%*S          ++       :*S#%########*                        
               +*****+*#################S S+         +        + %S S#######*                        
               +*****+*S################# S+         +       +% S?+#######?+                        
               +******+S###########?S####S#*        +        *S%#SS######S?+                        
               +******+%###########+S######%       ++        %##S##S?####*?                         
               +******+%##S#######S+########+      +        *#SSS### %@##+?                         
                +*****+?## %######S?########?     +         S#SSS###S?%SS?*                         
                +*****+*##:%############%S###+  +++        ?##SS#####S?+?*+                         
                +*******S# %############+ %##S*++*+       ?#S%SS#####@*+S                           
                ++*****+%#S########@@###?: ###S*++*+     *%%%*%*######S##*                          
                 +*****+?#########%** %#@?:+###*+++++  *++?+%%#?########%                           
                 +*****+*#########* +,S##S:*###?SSS%?+ **S##############?                           
                 ++*****+%#########@* ####+%###%*S@###??##S#############*                           
                  +*****+*##########%S####%###########%%#S+%######S##SSS                            
                  ++*****+S##########################%*%@@*+######%%#*#*                            
                   +*****+?#######################?#####%%#%######S?S%S                             
                   ++*****+SS%###################S+####*+ ?#S#########+                             
                    +*****+?S*?S######@###########?###@+?##*%########?                              
                     +*****+%S, S####%++##########S##S#?%#*?########S                               
                     ++****+*S%######  *############S*###? #########+                               
                      +*****+*########? ############*%###+:*#######*                                
                       +*****+?######@%:############%S###%% ####S#*                                 
                        +*****+%#######?####?######## +###*%####*?                                  
                         +****++%###########*?##S%###+:%##S##S##+                                   
                          ++***++?##########S?###*S##S%#####*?#?                                    
                           ++****+?####S?#######S?%#########SS*                                     
                            ++****+*S#@++######S**S#?#######?+                                      
                              ++***++?S?S##?%#######*S####%+                                        
                                ++***++?S##?%@######S##S*+                                          
                                  +++**++++**S#####SS?*+                                            
                                    ++++++ ++*****++                                                
                                        +++++++   
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_5():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                       +++++++++++                                                  
                                    +++*****+++*+ : ++                                              
                                  ++*****++*?%%SSS%%S%?*                                            
                                ++****+ **%S############S?+                                         
                              ++******+ %##################%+                                       
                             ++*+++*+*?+ #########S######%*SS*                                      
                            +*+    +*S?+S########S*#######S%##%:                                    
                          +++      *#############S+S##########@*:                                   
                         +++     :*SS###########@?:S###S#####?SS+*                                  
                        +++     ::S#S############S S##%:*###??####*                                 
                       ++        %#SS######?######*S@%+S+S########? +                               
                       ++      +S#SSS#####%:#########S #@#####?*S#+                                 
                      ++       %#SSS#####@*+########## %#########@#*?                               
                     ++       ?SSSSS##?%##?%?+#########+%########**S%                               
                     +       ++:+#S##S+S####S:########@? ########%+ * +                             
                    ++       ?:?SSS##%*###### %########S %#####?S@%?+*+                             
                   ++       ?++#SS####S######%S#########%?####? ##S?#?*+                            
                   ++       S?S#SS###@* S#######%#############*%####@???                            
                   +       *##SSS####?:*?#######+*###################%SS                            
                  ++       %SSSSS####*%S*#S %###% ######################*                           
                  ++      +S ?#S########S##?:%#####S*?##################%                           
                 +*       ??:SSS############%S###*?:++?S###############@%                           
                 ++       S+*#S#SS##############+ +:+++*?S#############?* +                         
                 ++      *#SSS## : S##########@S++*****++*S############%S+                          
                +*+      ?#SSS##SSS###########%+ ++******+*S@?%##########%                          
                +*+      %#SS################%    +*******+*#%*###########+                         
                +**      SSSS###############?+     +*******+%#SS*#########*                         
                +**+    +SSS##############S  +      +******+*#@*?#########*                         
               ++**+     ?###S?###########%?%+      +*******+%############+                         
               +****++ +??###S: S##%**%####@*       ++****+++?###########S                          
               +*******+S######*S###%?%#####+        +**+    +S##########?+                         
               +********SSS####@###########S         +*+      %##########%S                         
               +******+*#S+S###############%+        +*       ?#S#########S                         
               +******+*##*%###########SS##?+        ++       ?#S#########S                         
               +******+*###############??##?         ++       **S#########%+                        
               +******+*###############%?##?         ++       *%SS########?                         
               +******+*###############%?##?         +        *SSS########%                         
               +******+*#################%*?         +        :?SS%#######%                         
               +******+*S################S %        ++        + S% #######?                         
               +********S################# %+       +        *?+#*?@#####%+                         
               +*******+S###########%%####SS+       +        ?SSSS#######?+                         
               ++******+%###########*%######*      +        +S#SS##%?###%*+                         
                +******+%##S########*%######%     ++        ?#SSS##S S@@%*+                         
                +******+?#@+?@######?S#######*   ++        +SSSS####S%%S??                          
                +******+*## *@###########%S##S  ++         %##S######%*+*                           
                +******+*S# *@###########+ %##?+**+       %#S%SS######+%%                           
                 +******+%#S#########@###?: ###?++*++    *%?%*%*#######@%                           
                 +******+?#########S??+?#@%:*##S+++++  ** ?+%S#%########+                           
                 +******+*#########%: ,%##S:+##S?SS%%* **%#############S+                           
                  +******+S##########%:####+%###?*###@%?##S############%                            
                  +******+?##########S?####%##########S%##+*######%##%#*                            
                   +*****+*S##########################?%#@% ######?%S?S                             
                   +******+?######################S%###@S?#%######S?SS*                             
                    +*****+*S#S###################%*###S ++#%#######@%                              
                    +******+?#+?#######@##########%%###S ##??#######S+                              
                     +*****+*SS::S#####?+S############S#*#?*########?                               
                      +*****+*#?%#####+:+############*?###:S@######%                                
                      ++*****+?@#######% ###########S*###%: S#####S+                                
                       +******+%######@%:###########S%S##SS+S###%S                                  
                        +*****+*S#######*####?######@%:?#@%?####*+                                  
                         +*****+*S###########*%##%%##% +#####%#S+                                   
                          ++****+*S##########S?##S*###SS####??S+                                    
                           ++****+*%####S%######SS?S########S%+                                     
                            ++****++?##@++@#####%*?#S%######?                                       
                              ++***++*%#*S##?S######%?####%+                                        
                                ++***++*%###*S#######S#S*                                           
                                 +++***++*+**?#####SS?*+                                            
                                    ++++**+ ++*****+                                                
                                        ++++++++  
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_6():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                       ++++++++++++                                                 
                                    +++*****+++++ ::+                                               
                                  ++*****+++*%%SSSS?%%?*                                            
                                ++*****  *?S############S?+                                         
                              ++******* *%#################?+                                       
                             ++*++**++*%++########S######S*%S*                                      
                            +*+    +*?#?*#########??######S%##?:                                    
                           ++       %#############%+###########+:                                   
                          ++      :?#############@* #########%SS++                                  
                         ++      : SS#############% ###* S##S*S##%+                                 
                        +        +S#S#######?######+#@*??*########* +                               
                       ++       *S#SS######%:########@?*@#####%*S# :                                
                      ++        S#SS######@? #########%+#######S#@S*+                               
                      +        ?S%SS###??##%%%+########%+########??#*                               
                     ++       **,+SS###+%####S:######### %#######% ++ +                             
                    ++        %:?SS###S+###### %########**#####%S@%* +                              
                    +        ?* #SS#####S#####SS########S*S###% ##%?S*+                             
                   ++        S%%#S####@?:%#######%############??####@+%+                            
                   ++       *###SS####%,**####### %##################?#*                            
                  ++        %S%SS#####%?#*SS %###?+####################%                            
                  ++       +S++#S########S##?:%#####?*##################                            
                  ++       ?%:%##############%S##%*  +?S################+                           
                 +*        S* ####SS############S:+: ++*?#############S?                            
                 +*       +SSSS#@*,:%###########?+*****++*S###########%%*                           
                 +*       *#SS####SS###########?++*******+*#@?S##########+                          
                 +*+      ?#SS################?   +*******+?#%?@#########?                          
                +**+      S#S################?+    +*******+S#S%?########?                          
                +**+      SS###############S  +    +*******+?##*%########%                          
                +***+    +*####?S##########%?*      +*******+S###########?                          
                +****++++?*####+,?###**?#####+      +*****+*+%###########*                          
                +*******+%######??###S%?####S       +**++   +*S#########S+                          
                +*******+S#%################%       +*+      +SS########SS+                         
               ++*******+S#+%###############?       +*+       %###########+                         
               ++*******+S@%*############S##*       ++        %SS#########+                         
               +*******+*S##S###########%?##*       ++        ??##########+                         
               +*******+*###############S*##*       ++        *%S########%+                         
               +*******+*###############S*##*       ++        *#S#########+                         
               ++******+*S################S**       ++         ?SS%#######+                         
                +*******+S################S *       +         ++#**@#####%+                         
                +*******+S################# *      ++        ?**#+%######*                          
                +*******+%###########S%####S%      +         %SSSS######S*                          
                +*******+%###########%*######+    ++        +SSSS##?%###**                          
                +*******+?###########??######?    +         %#SS###% ###++                          
                +*******+?##%+#######?%######S   ++        *SSSS####S??S*+                          
                ++******+*#@? ############SS##? ++         S##S######%+*                            
                 +*******+S@* ############? %##*+*+       %#S?SS#####S*#+                           
                 +*******+%@S%############%: ##S*+*++    *%?%*%*########+                           
                 +*******+?##########S?**#@%:*#@?+++++ ** %*SS#%#######%                            
                  +******+*#########S: ,*@##:+##%%S%%* ++S@@###########*                            
                  +*******+S###########:%##@+?###*%@#@S?S@%############+                            
                  ++******+?###########*####%#########S%S@?+######%#S%%                             
                   +******+*S#########################%?#@# S#####?%?%*                             
                   ++******+?######################%S####*%S######S%S%                              
                    +******+*######################*%###+++S%########+                              
                    ++******+%#*?########@#########?S###+%@%*#######?                               
                     +******+*#S  %#####%*S###########S#%S%+S######S+                               
                      +******+?#**S####*, ###########S+###+*@######*                                
                       +******+%@@#####S% ###########?%### :?#####?                                 
                       ++*****+*S#######S:S##########S%###S??###%?:                                 
                        +******+*########+S###%#######+ S##*####++                                  
                         +******+?############*%##%S##? %####%S%+                                   
                          ++*****+?###########%?#@%*###S####??%+                                    
                           ++*****+*#####S%######SS?#########?                                      
                            ++*****+*S###+*@#####?*%#?#####S*                                       
                              ++****++?S#+%##?#######*S##S?+                                        
                               +++****+*?S#@#*S######S#S*                                           
                                 +++****++**?+S#####S?*+                                            
                                    ++++*** ++*****+                                                
                                       +++++++++      
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_7():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                       ++++++++++++                                                 
                                    +++******+++*+ , +                                              
                                  ++****++++*?%SSSS%%%?*                                            
                                ++******: ?%############S*+                                         
                              ++*******++?%###############S?                                        
                             ++*++***++?% *########S######?%%+                                      
                            +*+    ++*%#??#########*%#####S%##+ +                                   
                           ++      +*S#############*?########@#                                     
                          ++       :%############## *########SS%                                    
                         ++       :+SS#############**@#S ?###*%#S*                                  
                        ++        +SS########*#####S*## % S##S####++                                
                       ++        *#SS#######%:######### S#####S*SS:                                 
                       +        +S#SS#######? #########+%######S#@?++                               
                      +         ?%%S####%*##%?% ########*%#######%%S+                               
                     ++        *?, S####*?###@S:S######@?+#######? *                                
                     +         % *S#####*S##### %#######S S####SS@?+                                
                    ++        *?:SS######S#####SS########??###S S@%%%+                              
                    +         %S%#S#####%:?#######%###########??#####+*                             
                   ++        +S##S#####S:+*######S S#################?%                             
                   ++        ?S?SS#####S*#?%# ?###*?@##################+                            
                  ++         S?:S############%:%####S*%@###############*                            
                  ++        *S:*##############SS##?*:+*S##############@?                            
                  ++        %% S###SS###########@?  :+++?#############S+                            
                 +*+        SSS###S::*##########S++****++*S###########%%                            
                 +*+       +SSS####SS##########S*+*******+*##?S#########*                           
                 +*+       *#S#################+ ++*******+%@?%@########%                           
                 +*+       %#S################*   +*******+*S#S?%#######S                           
                ++**+      ?################# :+  ++*******+?##+S########+                          
                +***++   +++S###S?##########S?*    +*******+*###########S                           
                +*****++*+??S###%,+###?*?S###S+    +********+S##########S                           
                +********+?######S*S##S%?S###%      +**+   ++?##########?                           
                +********+%#SS####@##########?      +*+      *S#########%*                          
                +********+%@?*###############*      ++       +SS########@*                          
                +********+%#S+S###########S##*      ++       +SS#########?                          
                +********+%##S############*##+      ++       +*%#########*                          
                +********+S###############*##+      +        +?SS########*                          
                +********+S###############*##+      +        +?#S########*                          
                +********+S################S*+     ++        :+%#%S######*                          
                +********+%################# +     ++        + *# %#####S+                          
                +********+%################# +     +         %+%S+######%                           
                +********+?############%####S?    ++        +SSS########*+                          
                +********+?############*S###@S    +         ?#SS###+S##%++                          
                ++*******+?###########S*######+  ++         SSSS###?*@@%                            
                 +*******+*###+S######S*######?  +         ?#SS#####%?%?+                           
                 +*******+*S##:%###########S%##+++        +SSS#######? +                            
                 +********+%@S,S###########% ?#%+++      +S#S?SS#####?%%                            
                 ++*******+?##*############%::##?+++++  +*?*%?%?######@*                            
                  +*******+*###########%%*#@S:*##++++++*? %*SS#S#######                             
                  +*******+*S#########+:::###:+##?SS%?+ +%@@##########S                             
                  +********+%#########S#++@#@*?##S*S@@#%S@SS##########?                             
                   +*******+?###########*%###%S########%%#% S#####?#%%+                             
                   +*********S########################S?S#@+?#####?%??                              
                    +*******+?######################?####**#S#####SSS+                              
                    +*******+*S#####################*###? +%?S#####@%                               
                     +*******+%@??########@#########?##@%+@S*#######+                               
                     ++******+*#S++%#####S?%###########S#%S S######?                                
                      +*******+?@* ?####%::S##########??#@?:#####@%                                 
                       +*******+%#######S% S##########*###? +####S+                                 
                        +******+*#########:%##########%%###%*###?+                                  
                        ++******+?########+S###%######S:*##?S##S+                                   
                         ++******+%############*S##?##S++####?S?                                    
                          ++*****++?###########%?##?%#######%??                                     
                           ++******+?######S######S%?########*                                      
                            ++******+?####+*@#####?*SS?####S+                                       
                              ++*****+*%#@+%@#?######S*##S?+                                        
                               +++****++*%S#@S*#######SS*                                           
                                 +++****++**?%*#####S?*+                                            
                                   +++++***+:++****+                                                
                                       +++++++++ 
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_8():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                       ++++++++++++                                                 
                                    +++******+++*+ ::+                                              
                                  ++*****++++*%%SSS%?%?*                                            
                                ++******+:+?S###########%*+                                         
                               +*******++*%%##############S*                                        
                             ++*++****+*%?:*########S#####%%?+                                      
                            +*+    +*+*S#?%#########+######?SS+                                     
                           ++      +**S#############+%#######@S                                     
                          ++        :%##############:%########S?:                                   
                         ++        :+S##############+%@#*+###*?#%+                                  
                        ++         *SS########*S####%?@?***##S##@%++                                
                        +         *#S########S:S#######%+@####S?S%:                                 
                       +          S#S#####S##% #S######S ######%S#+                                 
                      ++         ?%?%####S+##S?% #######%*#######%S?                                
                      +         +%,:%####?*###@S:S####### S######* * +                              
                     +          %+ S#####?%@#### %#######+?####SS#* :                               
                    ++         *S:%#######SS####SS#######%*#### S@?%?                               
                    ++         ?S%S####### +######SS##########%*##S#%++                             
                    +         +S##S######+ *%#S###% ###############@%?+                             
                   ++         *#?%#######?S%?#+*### %#################?                             
                   ++         %S,%##########S#S:?####??###############%                             
                  ++         +#+ ##############SS#S*  +S##############S                             
                  ++         ?#+?###S%S###########+ :++*?#############?                             
                  ++         %#S####*, S#########%+****++*###########S%+                            
                  ++         SSS################%+*******+?##?########@%                            
                 +*+        *SS################S++********+%@*%@########+                           
                 +**+      +?#S################* ++*******+*#S#*S#######+                           
                 +**+     ++*S################+:+ +********+S@#+########*                           
                 +***++ ++**+%####?S#########S*+  +********+?###########+                           
                 +********+*%?####+,%##S**%###S    +*******+*###########+                           
                ++********+*#######??###S?%###?    +**++  +++%#########S+                           
                ++********+*##%###############*    +*+      +%#########S?                           
                +*********+*#S+S##############*    +*        *##########%                           
                +*********+?##??###########S##+    ++        *SS########%                           
                +*********+?###S###########*S#+    ++        **S########%                           
                +*********+?###############?S#+    ++        *?S########?                           
                +*********+?###############?S#+    ++        +%#########%                           
                +*********+?#################*+    +         :+%#?######?                           
                ++********+?#################     ++         * %S ######*                           
                 +********+*#################+    ++        +S+S%*#####S+                           
                 +********+*############SS###S*   +         *SSS#######?+                           
                 +********+*############??###@%  ++         %#SS##S+###+                            
                 +********+*############?%####S+ +         +SSS####*%##+                            
                 +********+*S##??#######*S#####*++         %#SS#####%*?+                            
                 +*********+%#@*+############%#S+         *SSS#######++                             
                  +********+?#@++@##########S ?#?++      +S#%?SS#####?#+                            
                  +********+*##??###########S :##*+++++ +*?*??%?#####@%                             
                  +********+*S##########SS*%@S:+#%+*+++*? %?S##S######*                             
                  +*********+%#########%::,%##  ##?S%?* +%@@##########+                             
                   +********+?##########S?:##@?*@#%?#@#%%@S%#########S                              
                   +********+*##########@?*###%S#######%?#S %#####?#??                              
                    +********+%########################%S#@?*@####?%?+                              
                    +********+?#####################S?###%+S######S#?                               
                     +*********S####################%*### *?%%######+                               
                     +********+?@S?#@###############S%###+##*S#####?                                
                      +*******+*##*+%######%%##########S#%#+%#####S+                                
                      ++*******+?@%,+##### :%##########+S@#:%#####*                                 
                       +********+S#S######? %#########%?@#S+ S##S?                                  
                        +*******+*########@ ?##########%S##S*###*                                   
                         +*******+?########+?###%#####@*:S@%%##S+                                   
                         ++*******+%############*S##%##%+%###?%*                                    
                          ++******+*%###########?%@#*S######S?*                                     
                           ++******++%######S######S?S######S+                                      
                             +******++%####**######*?#?S###%+                                       
                              ++*****++?S## ?@S%######*S@S*                                         
                               +++*****+*?SS#@S*######SS?                                           
                                 ++******++??S??####S?*+                                            
                                   ++++****+  *+***+                                                
                                       ++++++++++     
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_9():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                       +++++++++++++                                                
                                    +++*******+++*+ ,                                               
                                  ++*****++++*?%SSSS???*                                            
                                ++*******+:*%###########%*                                          
                               +********++?%%#############S*                                        
                             ++*+++****+*S?:?#############S%?                                       
                            +++    +**+?##%%########%*#####%%%                                      
                           +++     +*+*#############S+#######@?:                                    
                          ++        +:%#############%:S########*                                    
                         ++          +S############## S#S+%##??S*                                   
                         +         +*S#########*S####*S#+* ##S#@#*                                  
                        +          *#S#########:S@######+%#####?S?:                                 
                       ++          S#######S##S:SS#####@*?@####%SS:                                 
                       +          *%*?#####+%##?% ######S*S######SS+                                
                      +          +S::?####S+##### S#####@**@#####+++                                
                     ++          ??:S#####%?#####+%######%+#######+:                                
                     +          +S *########S####SS######S*S###+S#??+                               
                    ++          *#SS#######+:S#####SS#########%+##S#+                               
                    ++          %#########?:*?#S###?+###############?*                              
                    +          +S%?#######%%#*#**##S ################S                              
                   ++          ?# +############S:?###%*###############+                             
                   ++          S%,%#############SS@% :+S##############*                             
                   ++         +#%+####%%S########@S:: +*?############S+                             
                  +*+         ?#S####S::*#########*+***++?###########%*                             
                  +*+        +%#S################?+******+%##?#########+                            
                  +*+       +*SS################S++******+*S#*S@#######*                            
                  +*+      ++*S#################*+********+?#SS*#######?                            
                 ++**+    +**+%#####@##########+:+********+*#@%+#######?                            
                 +****++++****+####S?##########*+ +********+%##########?                            
                 +**********+%*S###S: ###?*?###%  +********+?##########?                            
                 +**********+S######S*S##S%?###?  +**+++ ++**S#########*                            
                 +**********+S#SS####@#########*  +*+      +*S#########%+                           
                 +**********+S@?*##############+  +*+        %#########S+                           
                 +*********+*S#S+S#############+  +*         %S########S+                           
                 +*********+*S##S###########%%#+  ++         ??########S+                           
                 +*********+*###############%?S   ++         ?%########S                            
                 +*********+*###############%?S+  ++         *SS#######S                            
                 +*********+*#################*   ++         :*SS?#####S                            
                 +*********+*S################+   +         +* S?+#####?                            
                 +**********+S###############@*   +         *%+#*%#####*                            
                 +**********+S############%###S+ ++         %SS#######S++                           
                 +**********+%###########S*###@? +         +SSS###?*@@?                             
                 +**********+%###########S*####S++         *#SS####*##?                             
                  +*********+%###*S######%*#####*          SSS######?*+                             
                  +*********+?###:S###########%#?         ?#SS######%++                             
                  +*********+*##S,S###########+?#*+      +S#%?%S####SS?                             
                  +**********+S#S S###########+,S%++++++**?*??%%######                              
                   +*********+%###########S%?##:+#*++++*% S%S########S                              
                   +*********+?##########+:,*@#+ #S%S%? +%#@########@?                              
                   +*********+*##########%S:%##?*@#?%@#SS#S?########S*                              
                    +********+*S########### S##SS######S?S# *@###S?S*+                              
                    +*********+?#######################S%##S*#####?%*                               
                     +********+*#####################%%#@S ?#######S+                               
                     +*********+%####################?%#@*+?%?####@?                                
                     ++********+?##?S################%S#@??#*S#####+                                
                      +*********+S#?*%######S%##########%SS**@####?                                 
                       +********+?## :%####*,*#########%*#@ +####S+                                 
                       ++********+%#%S#####?+?@########?S##* %##%+                                  
                        +********+*########@+*@########S?###*##S+                                   
                         +********+?########**###%###### +#S%##%                                    
                          +********+S############*S#S%##**###??+                                    
                           +*******+*S###########?%@S*#######?+                                     
                            +*******+*S#############%?######%+                                      
                             +*******+*S####?*#####S*%#?###%+                                       
                              ++******+*%### ?@S%#####S*#S*                                         
                               +++*****++*%#S#@%*######S?+                                          
                                 ++******++*?%S*%###S%*+                                            
                                   ++++****++:+****++                                               
                                      +++++++++++   
                                                                            """)

print("\n\n\nrunning animation.....")


def print_pattern_10():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                             ++                                                     
                                       +++++++++++++                                                
                                    +++*******+++*+ ::                                              
                                  ++******++++*%SSSS%*?+                                            
                                ++******** :?S##########S*                                          
                               ++********++%%%############%*                                        
                              +*+++*****+*S?:?#############%*                                       
                             ++    +***+?##%S########??####%%?                                      
                            ++      **+*#############%*#######*                                     
                           ++       *+:%############@* ########                                     
                          +         * +##############S ##?*##%?%+                                   
                         ++         **S#########*%####*#%+ %@SS@S+                                  
                        ++          *S##########:%@#####S+#####%S+                                  
                        +          +%########S##:SS#####S #####?S?:+                                
                       +           *S**#####??##?S ######??######S?                                 
                      ++           S*,*#####+%#### S#####S ######+*                                 
                      +           *S:?@#####*#####+?###### S#####S :                                
                     ++           S? ########%####SS######??###+S#**                                
                     ++          +SSS#######%,?#####S#########%+##SS                                
                     +           ?##########:+*####@*?#############S++                              
                    ++          +SS*S#######?#?S?+##%+##############S+                              
                    ++          *#%,S############ *@#S?%############@?                              
                   ++          +%#+ #############SS#*: %#############%                              
                   ++          +S#+%###S%S########@*::+*?############?                              
                   ++         +*SS#####?: S#######S++**++?##########S?                              
                   ++        ++*S#################*+*****+%#S?########*                             
                   +*        ++?S################%++*****+*##*########?                             
                  +**+      +*+%#################*+*******+%#SS?######%                             
                  +**+    ++**+*################+:+*******+?#@?*######S                             
                  +***++++*****+?####%%#########*+********+*##########S                             
                  +**********+*%?####?,?##S**S##? *********+S#########S                             
                  +**********+*#######%*###S?S##* +**+++++*+?#########%                             
                  +**********+?##S##############+ +*+      +*S########%+                            
                  +**********+?##+%#############+ ++        *S#########+                            
                  +**********+?##%*###########SS+ ++        +SS########+                            
                 ++**********+?###S##########S?S+ ++        +?%########*                            
                 ++**********+%##############S*S  +         +%S########+                            
                 ++**********+%###############*S  +         +?#########+                            
                  +**********+%################* ++           ?#%%#####+                            
                  +**********+?###############@* ++         *++#+%####S+                            
                  +**********+?###############@?:+          %??#*#####%                             
                  +**********+?############SS##S++         +SSS###S###*+                            
                  +**********+*############?%###?+         *#SS###+%@#                              
                  +**********+*############*%###%          %#S####%?#S                              
                  +**********+*###S?#######*S###S         +SSS#####S*+                              
                  ++*********+*S#@? #######S###S#*        %#S#######*+                              
                   +**********+%#@* ###########**%++     *##%%?S####S%                              
                   +**********+?##*+###########*,%?+++++**?+?%?S####@*                              
                   +**********+*############S?## +S+++++% S%#########+                              
                   ++*********+*##########%:::#@*:#%%%%+ %#@########S+                              
                    +**********+S#########S%*+##%+@S*#@#S##?#######S?                               
                    +**********+%##########@*?##S%######?%#++####%%?+                               
                     +*********+*#######################%###*#####??                                
                     +**********+S####################?#@#++#######?                                
                     ++*********+?####################*##% %%?#####+                                
                      +*********+*##SS################%##S+#*%####?                                 
                      ++*********+%@%+%#######%#########%SS%+####S+                                 
                       +*********+*#@?,*####%: #########*S@?:S###?                                  
                        +*********+%@S?#####%+*########S?@#%+?@#*                                   
                         +********+*S#######@? #########%%##?S@S+                                   
                         +*********+?########% ###S#####?:S#%#S*                                    
                          +*********+%########S###*##%S@%*S##?*                                     
                           +********+*S###########?%@%*######*                                      
                            +********+*S############S%%#####%                                       
                             +********+*S####?+#####%*S%S@#?                                        
                              ++*******+*%##@+*@SS#####?SS+                                         
                               +++******++?S#S#@??#####S?+                                          
                                 ++*******++*%SS*S###%*+                                            
                                   +++******++:*+**++                                               
                                      +++++++++++  
                                                                            """)

print("\n\n\nrunning animation.....")

def print_pattern_11():
    
             print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                            +++                                                     
                                       +++++++++++++                                                
                                    +++********+++*  :                                              
                                  ++*******+++*?%SSS%**+                                            
                                 +*********  %S#########S*                                          
                               ++*********+*S%%###########%+                                        
                              +*+++******+?#?:%############%+                                       
                             +++   +****+?##SS########*S###%?*                                      
                            ++      +**+*#############*%######                                      
                           ++       +*+:?############# *@####@S:                                    
                          ++        +*  #############@?*@#*##S??                                    
                         ++         ++*S#########?%###S*@*++#SS@?                                   
                         +          +*S########## ?@####@*%####SS                                   
                        ++         ++?########S## %S####@*?@###?S+                                  
                        +          +*S?+%####S+##%S #####S*######S+                                 
                       +           +%%,+S####?*#### S####@+%####S++                                 
                      ++          +*S+ @#####?%@##@+?#####**@####?: +                               
                      +           +?S %#######SS###SS#####%*###*SS*+                                
                      +           +%#S########:+#####S########%+##S*                                
                     ++          +*S#########* *S####+S###########@?                                
                     +           +?#%*#######%S%%% #@*?@############?                               
                    ++          ++%# +############ *@#%?############S                               
                    ++          +*SS,%############SSS  %#############+                              
                    ++         ++?#%+####%%S######## : *%###########S                               
                    ++        +*+?####### :*#######%+**++%##########%+                              
                   +*+        +*+%#################*****+*S#S%#######%                              
                   +*+       +*+*S################%+*****+?#S*@######S                              
                   +*+      +**+*S################++*****+*S#S%%######+                             
                   +**+   ++*****%###############*:*******+%#@+%######+                             
                   +***+++*****+*+S####?S########*+*******+?##########+                             
                  ++***********+%?%####+:S##?*%##%+*******+*##########+                             
                  +************+%#######*%##S%%##?+*++++++*+%########S+                             
                  +************+S##S####@########*++      ++%########S*                             
                  +************+S@S+S############++        +?#########*                             
                  +************+S##*%##########SS++         ?S########?                             
                  +***********+*S##SS##########?%++         ?*S#######?                             
                  +***********+*S##############?%++         ?%S#######*                             
                  +***********+*###############?%++         +%########*                             
                  +***********+*S###############?+          :+%#*#####*                             
                  +***********+*S##############@?           ? %S #####+                             
                  +************+S###############?:         +S*SS?####S+                             
                  ++***********+S############S##S          *#S####S##?                              
                   +***********+%############*###*         %#S###S #@*                              
                   +***********+%###########S*###?        +SSS####?%#*                              
                   +***********+%###%S######%*##@S        *#S######?+                               
                   +***********+?### *@######S##SS+      +S#S######S++                              
                   +***********+*###:?@#########?+?      ?##?%?#####S+                              
                    +**********+*S## %#########@?,?*+++*?*?+?S?S####%                               
                    +***********+S############%S@+ ?++++? SS########?                               
                    +***********+%##########+:,%@?:#?%%* %##########*                               
                    +***********+?##########%?:S#S+#%?@#S##*S####S#?+                               
                     +**********+*##########@S ##S%#####??#? ####%%+                                
                     +***********+S#####################S###?####S?+                                
                      +**********+?###################S?@#* SS####S+                                
                      +**********+*###################%?## ?%*####?                                 
                      ++**********+%##S###############S%#@*S?%###S+                                 
                       +**********+?##+?#######SS########%SS ####?                                  
                        +*********+*S#S::S####+:S#######%*@S:%##S+                                  
                        +**********+?##*%####S+ ########?S@S**##+                                   
                         +**********+S########S:#########*S@?%@?                                    
                          +*********+*########S:###S##### +#S#%+                                    
                          ++*********+%########S###*##?##?%##?+                                     
                           ++********+*S###########?S@?%@####*                                      
                            ++********+*S############S?SS###?                                       
                             ++********+*#####%+#####??S?##*                                        
                              ++********+*S##@+*@%S###@S?S+                                         
                                ++*******+*?S#SS@?%####S?+                                          
                                 ++*******+++?%#S?S##%*+                                            
                                   +++*******+  *+**+                                               
                                      ++++++++++++                                                  
                                          ++        
      
                                                                          """)

print("\n\n\nrunning animation...")

def print_pattern_12():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                           +++++                                                    
                                       +++++++*++++ +                                               
                                    +++********+++*+ :                                              
                                  ++********+++*%SSS%?*+                                            
                                 ++*********: %#########S*                                          
                                +**********+*S%%##########?+                                        
                              +++++*******+?#?:%###########%+                                       
                             +++   +*****+?##SS#######S*###S*+                                      
                            ++      +***+*#############*S####%                                      
                           ++       +***:*@############:S####@?:                                    
                           +        +**+ S#############+%@?%##?+                                    
                          +         +*+*S#########??###?%S  SS%#+                                   
                         ++         +*+%#########@+*@####S+S###S?                                   
                         +          ++*S#######S#@+?#####S ####?%                                   
                        +          +*+%%+*#####+S@%% #####*S#####?                                  
                        +          ++*S :?####S+#### %####%*####%+                                  
                       +           ++%%,S@####S?###@*?####% #####+,                                 
                       +          +**S?+########%###SS####S*###*#%+                                 
                      ++          ++?S#########*:S####S#######%+@S%                                 
                      +           *+%#########S:*?####+############                                 
                     ++          +**S#*%#######%#?S ## %###########%+                               
                     ++         +*+*#S,%###########++@S?############+                               
                     +          +*+?#*:#############S*:?############*                               
                    ++         +**+%#??####?%######@%,:*%###########*                               
                    ++         +*+*S######S::S######*+*++S##########*                               
                    +*        +**+*S###############S+***+*##%S######S+                              
                    +*       +***+?################?+****+%@??@######+                              
                    +*+     +****+?################++****+*##S?S#####*                              
                   ++*++  ++*****+*S#####@########?:*****+*S##+######*                              
                   +****++**********#####*########*+******+%#########*                              
                   +************+*S*#####: ###*?S#?+******+?#########*                              
                   +************+*#######S*S##S?S#?++++++*+*S########*                              
                   +************+*##SS####@#######*      +**S########*                              
                   +************+*##?*############+       ++%########%                              
                   +************+*##S+S##########S+        +SS#######%                              
                   +************+*###SS#########%?         +%*#######%                              
                   +************+?##############%*         +%%#######?                              
                   +************+?##############%*         +*S#######?                              
                   +************+?###############?           +#S?####?                              
                   +************+*##############@%:        +? #**@###*                              
                   +************+*###############S:        *S*#?S@###*                              
                   +************+*############SS##         %#####S##S                               
                   +************+*############?%##*        SSS###?*@%                               
                   +************+*############?%##?       *#S#####?S?                               
                    +***********+*S###%#######*S##%       %#S######*                                
                    +************+S##S:S######S###%      *SSS######*+                               
                    +************+%##S,S#########S++   + %S#?%*#####*                               
                    +************+?##S:S########@S:+*+**?**+?S?#####+                               
                    ++***********+*###S########S%@*:+++*? SS#######S                                
                     +***********+*##########%:: @%:%?%?:?#########%                                
                     +************+S#########S? *@S+#?S#S##*%####%S+                                
                     ++***********+%##########@+%##%####S*#% ####??                                 
                      +***********+?####################SS#S?####S?                                 
                      +***********+*###################?S@?:%S####*                                 
                       +***********+%##################?S@**S*###S+                                 
                       +***********+?##################%S@%%?%###?                                  
                       ++**********+*S@%*S######SS#######%S# S###+                                  
                        +***********+%##*:*####?:%#######*S#:*##?                                   
                         +**********+*#@%+S####*:S#######?##?*#%                                    
                         ++**********+%######### %#######%?#%%@*                                    
                          +**********+*######### %##S####%:S#S?+                                    
                           +**********+?########S##S*##?#S?##*                                      
                            +*********+*S###########?S#*S####+                                      
                             +*********+*S############%?S###*                                       
                              +*********+*#####%+#####?%?S#*                                        
                               +*********+*S##@*+@%S###@??+                                         
                                ++********+*%#@SS#*S###S?+                                          
                                 +++*******+++?S#%?##S?+                                            
                                   +++*******++:**+*+                                               
                                     ++++++*++++++                                                  
                                          ++++                                                      
                                                 
                                                                            """)

