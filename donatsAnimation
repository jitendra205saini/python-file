def print_pattern_104():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                   +++++++                                          
                                                +++******++++                                       
                                              ++++***********++                                     
                                           ++?SS%?++***********++                                   
                                          ?S*+@#@#%++***********++                                  
                                         %#+%?##%#@S++***********++                                 
                                        *#% S#S%+###% +************+                                
                                      +%%#S%##*:*####? +************+                               
                                      %#%S#*%#S%SS####+ *************+                              
                                     *#%?##++SS#SS####% +************++                             
                                    +S#%%%#S? S#S#%:?@#+ *************+                             
                                    *##S*SSSS:%#SSS?S##% +*************+                            
                                    *+##S?::%*%#SSS#####++*************++                           
                                    *%###%+*+*SSSSSS####* **************+                           
                                   ++S####*S? #SSSSS####% **************+                           
                                  +S?*##S%?S**#SSSSS#####++**************+                          
                                  *##*?@??##?%#SSSSSS####?+**************+                          
                                  *##S+%??#SSSSS#S#*+####%+***************+                         
                                  ?@###S####SSSSSS#+ ####S*+**************+                         
                                  *@########SS## *#++#####*+**************+                         
                                 +%##########S?S ?#%%#####?+***************+                        
                                 ?@??###S####?*S SS##%:S##%+***************+                        
                                 ?@S*###?*###?SS%SSS#+ ###S*+**************+                        
                                  ?###%%#:S%%#SS#SSSS%S####*+***************+                       
                                +%%##@**@%S+?#SSSSSSS######*+***************+                       
                                +#####SS#@?+*#SSSSSSS######?+***************+                       
                                +####%###?*+*SSSSSSS######S*+***************+                       
                                *###%%### +*+%#SS#SS######+,+***************+                       
                                *#S??####%+*+%#SS?SS#######%+***************++                      
                                *@??##?##*+*+?#SS:%########S*****************+                      
                                *###SS*##++*+?#S% *#########*+***************+                      
                                ?##SSSS##  ++*#%+++#########*+***************+                      
                                ?@#SSS%#S   +*#??SS#########*+***************+                      
                                ?S#SSS?SS   +*#%SS##########*+***************+                      
                                *?#SSS##S    +##S###########*+***************+                      
                                ?%#SSSS#S     S############# +***************+                      
                                ?@*SSSS#S     S############S:+***************+                      
                                ?@+%#SS#S     S#############:+***************+                      
                                *#%SSSSSS     S##S+S######## +***************+                      
                                *##SSSSSS+    S##*?#########*+***************+                      
                                *##SSSSS#+    S#############*+***************+                      
                                *%%#SSSS#*   +#########?*###*+***************+                      
                                +**#SSSS#?   +#########*?###*+***************+                      
                                 ?+SSSSS#%   +#########+S##S*+***************+                      
                                 % SSSSSSS+  *##%+#####S###S*+**************++                      
                                 %%#SS?%##? :*@#?%#########S+***************+                       
                                 %###??SS#%::?@?%*S########%+***************+                       
                                 ?###%?#SSS+ %# %??########%+***************+                       
                                 *###SSSS#** %# %@#########?+***************+                       
                                  S#SSSS##*+??S %##########?+**************++                       
                                  %@?%#S*%S%S+*+###########*+**************+                        
                                  ?@%S##? S#S+# %##########*+**************+                        
                                  *###%**+####@*+#####%###S+**************++                        
                                  +###+:*S######S####S:+##%+**************+                         
                                   %%%?%S#############S?##?+**************+                         
                                   ?S#S?%@?S#####* #######*+*************+                          
                                   +?S**##%%####S:%######S+**************+                          
                                    +*+%@#%%?%###S#######?+*************++                          
                                    +*%*###S*S########SS#*+*************+                           
                                    +%S?####S#########?*%+*************+                            
                                     *SS################?+*************+                            
                                      %@###############S*+************+                             
                                      +################?+************++                             
                                       *?%S%######*+##S*+***********++                              
                                        *#%?######:%@#*+************+                               
                                         ?SS#S+###S##?+***********++                                
                                          *S#@ ?@#%S*+***********++                                 
                                           +?%S###*+++**********+                                   
                                              +*?****+********++                                    
                                                 +++******++++                                      
                                                   ++++++++  
                                                                            """)

