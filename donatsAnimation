def print_pattern_124():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                ++++++++                                            
                                           +++++**+++++++++++                                       
                                        + , ?%%SSSSSSS%?***+++++                                    
                                      ++??%######@#######*+%*+++++                                  
                                    +*%S#######?**S######  ?#S?*+*++                                
                                   *%###SSSSSSSS%%S#####S:SS###S%***++                              
                                 +?###SSSSSSSSSS###%S####*######@S++*++                             
                                +S##%?#SSSSSSSSSSS?+%#######S##%+ +%*+*++                           
                               *##SS%+SSSSS##SSS#%*S##SS###S:*##%*%@#?+*++                          
                              *S#SSS+*SS#S  ?SSSSSS#%*+%####S ?@#@####?+*++                         
                            +%#SSSSS ?##?*%**SSSSSSS%?%SSS##@##@**#####%**++                        
                           +S#SSSSS#**##? ###SSSSSSS###SS+: %?%@%:S####* +*++                       
                           %#SSSSSSSSSSSS:%S#SSSSSSSSSSSS%? %?:+#*?####%+?**++                      
                          ?##+%#SSSSSSSSS%% %#SSSSSSS#SSS##**@S*#%*#####@#?+*+                      
                         ?##S %SSSSSSSSSS##*:S#SSSSSSS ?#S#%+##@@+*@#######*+*+                     
                        +#S#%***#SSSSSSSSSS#++*S###S#+ SSSSSSS### %########S*+*+                    
                        %%*#S#? SSSSSSSS#####S?######*S#SSSSSS###?#########@%+*++                   
                       +#*%#SSS+SSS#SS##################SSSSSSS############SS?+*+                   
                      :%#?SSSSSSSSS?S####################SSSSSS############ ?#**++                  
                      *#S*?###SSSSS +#####################SSSSSS########%+#+?@?+*+                  
                     +S#* *#%*S#SS##*######**S#############SSSSS####S*%@*+@%%@S**++                 
                     *##?S?SS  %#######?S#S:*%??+*?%S#######SSSSS##?+?S#+?####%+**+                 
                     ?#S#SSS#S?S######* ##%:++++++++*?S#####SSSSS##S####S####S:***+                 
                     ##SSSSSSS#S######*S#?*++****++++++?SS###SSSSS############%#*+*+                
                    ++?#SSSSSSS####S####*++***+++   ++++*+S##SSSSS##############%+*+                
                    ??%SSSSSSSS###?+S@S +***++         +++?#S#SSSS##############S**+                
                   +S@#SSSSSSS##??S###? +**++            +*+*#SSSSS#############S  *+               
                   +###SSSSSSS*+S?###%+***++             ++ S#SSSSS########S#####+ *+               
                   +###S#SSS#*+%#@S*#*+***+               +*S#SSSSS#######S:#####%+*+               
                  + +#?+?SSSS#####*%%+***+                 +?#SSSSS#######S *####S+*+               
                   %S#S%%SSSS#####S#?+***+                 +*S#SSSSS#####**%+####S**+               
                   %@####SSSS#####S#*+***+                  +%#SSSSS####**#######S**+               
                   %###SSSSSS#####******+                   +?#SSSSS####S#########**+               
                   S###S#%*#S######%+***+                   +*SSSSSS##############+*++              
                  +S#####%*SS######%+***+                   +*SSSSSS############## **+              
                   S####SS*SSS#####%+***+                   ++SSSSSS##############+*++              
                   %#####SS#%+#####S+***++                   +SSSSSS#######S**####*++               
                   %#####SSSS:S####S*+***+                  ++#SSSSS#######*?#####**+               
                   %###?##SSS+%#####*+***+                 + +#SSSSS############%***+               
                   ?###*##SSSSS#####%+***++                +:+?*#SSS############*?**+               
                    ##S?###SSSSS####S*+***+                 :? %#SS#############?%+*+               
                    S######SSSSS#####?+****+                *%+#SSS#######%?#####%+*+               
                    %######%*SSSSS####*+****+            + +SSS#SSS######?+%#####?+*+               
                    ?######?, SSSSS###%++****+         ++  %#SS*SS######S%?%S####**+                
                    *#%  +##?:%SSSSS###?  ++**++++  +++   %#SSS +######% #?*%###%+*+                
                    +S#S+*@## :S#SSSSSS#%    ++++++++    %#SSS#S S@###@? #######?+*+                
                     ?#@*S###* SSSSSS?+S#S+:           +%#SSSSS#S???###?+#######*++                 
                     +#######?%#SSS#%+%SS##?+       : %*?%SSSSSSS??**#S S#####@S+*+                 
                      %@########SSSSS%*SSSS##S?: +*:+ ?*?*#SSSSS##*: S# *####%***++                 
                      +S#########SSSS#SSSSSS###++SS%#SSS%%#SSSS###?%###% ####S+ *+                  
                      :*##########SSSSSSSSSS%%%SS%S#SSSSSSSSSS################?*++                  
                       +#% ?#######SSS?SSSSS?*?S#?*#SSSSSSSSS############**##%+*+                   
                        *S?*########SS*SSSSS##SS#%:SSSSSSSS#############+ S##*++                    
                         S# #########S?SSSS#??+ %S*SSSSSSS#SS###########%###?+*+                    
                         +S+S############SS#*+S++SSSSSSS###%*###S?#########*+++                     
                          +S############S*S#**##S**SS#######*S##+?########S**+                      
                           *###%?######S+%####S***S#########%S@%*#########*++                       
                            *###*S#####S%####@?:S########################?+++                       
                              ?@SS##?S##S S###  +#####################%S*+++                        
                              +S@###%*##S::S#### *###################? *+++                         
                               +%%##*+S##%+?###?*##################@%:+*+                           
                                ++S#%*%########S#########S##########?**+                            
                                  +%####S%###############*%###%S##%*+++                             
                                    *S###*########%*+%###*S###?:%*+++                               
                                     +*%#%########SS#####S####S*++++                                
                                        +?S########@#######S?*+++                                   
                                           +*?%S?%SSSSS%??*++++                                     
                                               ++++  +++++    
