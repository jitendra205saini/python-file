def print_pattern_125():
    
    
        print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                ++++++++                                            
                                           +++++**+++++++++++                                       
                                        ++: *%%SSSSSSS%?***+++++                                    
                                      +*?%%S######@######S ??+++++                                  
                                    +*%########S**?######%:+#S?*++++                                
                                  +*S###SSSSSSSS%%%#####@*+S###S%*+*+                               
                                 +%##SSSSSSSSSSS###S%####%?@####@#*+*++                             
                               +*S#SS*SSSSSSSSSSSSS*+S######S###**+?*+*+                            
                               *##SSS+?#SSSS##SSSS*?S#SS####*:S#S*+##?+*++                          
                              *#SSS#*+%#S#*:+%#SSSS#S?*+S####*:S#@####%+*++                         
                            *S#SSSS#? #SS+%?+%#SSSSSS?%SS####@S##+S####%**++                        
                           *##SSSSSSS+SSS ?###SSSSSSS##SSS  +S?#@++####%++*++                       
                           S#SSSSSSSSSSS#*+SSSSSSSSSSSSSSS?* S :%S S###S++**+                       
                         +%##+*#SSSSSSSSSS%++#SSSSSSSS#SSS#S+S#?*@*%######%+*+                      
                         %#SS %SSSSSSSSSSS#S:*##SSSSS#% SSS#??####:%#######?+*+                     
                        ?#%SS*? #SSSSSSSSSS#% *%#####S:*#SSSSS###S:S########*+++                    
                       +#?*#S#S:%#SSSSSS######%%#####S*SSSSSS####S*#########S+*+                    
                      :*#*%#SS#*%#S##S###################SSSSS##############S%+*+                   
                       #S%SSSSSSSSS*S#####################SSSS#############% #*++                   
                      %#S+?#S##SSS#*:S#####################SSSS##########*?S #%+*+                  
                     *##+ +S%+%#SS##?%#####?+%@#############SSSS#####S?##:%#*##*++                  
                     %#S?S?SS+:?#######?%##  %??+*?%S########SSS####*+?#S:#####++*+                 
                     S#S#SSSS#%%######%:S#S  *+++++++*%######SSSS###S####S####* **+                 
                    +@#SSSSSSS########?%#%*++***+++++++*SS####SSS#############%S?+++                
                    * ?#SSSSSSS####SS###?++***+++   ++++***###SSSS##############S+*+                
                   +S?SSSSSSSS###@%+%@S +***++         ++++SS#SSSS###############**+                
                   *##SSSSSSSS#@%*S###? +**+            ++*?:S#SSSS##############+ +                
                   %@#SSSSSSSS?+S?S##%+***+               + ?##SSSS########S#####* ++               
                   ?##SS#SSS#?+%#@#*S*+**++               ++?##SSSS#######@*+@###S**+               
                  + *#?+?SSSS#####?%S+***+                 +*S#SSSSS######S%:S####**+               
                  +S%#S%%SSSS#####S#?+**++                 ++%#SSSSS#####S+%+%####**+               
                  +######SSS######S#*+**+                   +?#SSSSS####S+%#######*++               
                  +####SSSSS######******+                   +*SSSSSS#####S########*++               
                  *####S#%*S#######%+***+                   ++S#SSSS##############+++               
                  *#####S%+S#######%+***+                   ++%#SSSS##############+++               
                  *#####SS*S#######%+***+                   + ?#SSSS##############*++               
                  +#####SSS#% #####S+***+                   + %#SSSS########?+%###*++               
                  +######SSSS:S####S*+**+                   + %#SSSS#######%*S####*++               
                  +####%##SS#+%#####*+***+                   :SSSSSS############S***+               
                   S##S*##SSSS######%+***+                 + :%+SSSS############%***+               
                   *##S*###SSS######S*+***+               + :+*+#SSS############%%**+               
                    ###S####SSS######?+***++              + +%*%#SS#######S*?####S+*+               
                   +#######S*S#S######*+***++            +  %#S##SS#####@S+?S####?++                
                   +S###@##%, S#SS####%+****++         ++  ?#S#%*#S#####SS%??S###*++                
                    %@%++*##?:?#SSS####% ++***++++  +++   *SSS#%:?######+*S%?S##S+*+                
                    *#S? +### :%#SSSS###%   +++++++++    ?SSSSS#?+#####@ *@#####%++                 
                    +S#@*%###?:SSSSS#%+%#S+:           +?#SSSSSS#%**%###+%######*++                 
                     ?#######%*#SSSSS+?SS##?+       ::??*?SSSSSS#S%?**#*+######S**+                 
                     +S#########SSSSS%*%#SS##S%  +*:+++?%*%#SSSS### :*#S:%####+ ++                  
                      *##########SSSS#SSSSSSS##? ?S%SSSSS?SSSSS####%####??@###? ++                  
                      :%##@#######SSSSSSSSSSS%%SSS%#SSSSSSSSSS###########@####?*+                   
                       *#S*?########S#%%#SS#%**%#S %#SSSSSSSS############?:S@S+*+                   
                        %%+*##########??#SSSS#SS##+*#SSSSSSS############? %##?++                    
                        +##+S#########?SSSSSS*% ?#%*#SSSSS##%###########S#@#*+++                    
                         *#+%#############SSS:%% ?SSSSSS####*%###?%########%+++                     
                          ?S#############?%#S S##%+%########S*##%+S#######S**+                      
                           %###S?#######?*###S#%**%##########%##?S########*++                       
                            %###*%######?#@####:*########################?++                        
                            +*?@S?##S%###*?###%::?###################@%??++                         
                             :+#####S*S##*,*####%:S##################%:+*+                          
