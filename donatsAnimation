def print_pattern_121():
    
             print("""                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                                                                    
                                                +++++++++                                           
                                            +++++++**+++*+++++                                      
                                         ::+?%SSSSSSSS%?**++**+++                                   
                                       +**?####@######@%:SS?*++*+++                                 
                                     +*S#####S**%###### **###%**+**++                               
                                    *S###SSSSS%%%#SS###:S######S*++*++                              
                                  +%##SSSSSSSS###%SSSSS%#####SSS+?*+**++                            
                                 *#S%SSSSSSSSSSS?+%#SSS##S?##S+:+@S?++**+                           
                                %##%+SSS###SSSS%*%#SSSSS#%:+###SS###%++**+                          
                               ?#SS?+SS#?+?SSSSSS#%++%#SS#S+%#S######%*+**+                         
                              ?#SSS ?SS?%+ %#SSSSS%%SSSSSSS### *@####S%++**+                        
                            +S#SSS#**## ?##SSSSSSS##SSS ::? ?#%:S####*:+*+**+                       
                            %#SSSSSS%S#* ##SSSSSSSSS#SSS%+S*:*#*%#####S#S*+**+                      
                           ?#?SSSSSS#SSS?**#SSSSSSS#SSS##+?#S%#+%########%+***+                     
                          *#S SSSSSSSSSS#%:?##SSSSSS %#S#%?###S:S#########?+**++                    
                         +##?+*SSSSSSSSSS#? *%SSSS#+ #SSSSSSS#% ###########*+**+                    
                         ??SSS %#SSSSSSSS##S?%####S*S#SSSSSSSSS%##########@S+***+                   
                        +%*#S#**#S#SSSS##############SSSSSSSSSS##########??@?+**++                  
                       :?%SSSSSSSS%SS#################SSSSSSSSSS######SS@+*@S*+**+                  
                       +#%%###SSSS++####################SSSSSSSS#####@++@*+@#?+**+                  
                       %S *#%S#SSSS+S#####?*S@###########SSSSSSSSS*+##:?@S%##S*+**+                 
                      +#??*S?:*SSS####%S##  S%?+*?%S#####SSSSSSSS+?S## #####% ?+**+                 
                      *###SS#%*SS####S:S##  *++++++*?S####SSSSSSSS########## ?S****+                
                      %@SSSSS##S#####%*@#?++*****+++++?%###SSSSSS###########S##*+**+                
                      *S#SSSSSSS#######S*+*****+++  ++++*##SSSSSSS#############?+**+                
                     +*SSSSSSSS###**#@% +****++        ++SS#SSSSSS#############S****+               
                     %@#SSSSS##S*%S###* +***++          ++ #SSSSSS#############%: **+               
                     S##SSSSSS+?%?###%+*****+            :?##SSSSS######SS#####S  **+               
                     S###SSSS++S##%*#*+****+              +##SSSSS#####@? ######%+**+               
                    :*#**SSSSS####*SS+****++               %#SSSSSS####S%:%#####S+**++              
                    *S#S%SSSSS####S#?+****+                *#SSSSSS###S+%**#####S+***+              
                    *####SSSSS####S#*+****+                +SSSSSSS##S+%########S*+**+              
                    *###SSSSSS###S*%*+****+                 %#SSSSS###S##########*+**+              
                    ?####%*SSS#####S*+***++                 %#SSSSS##############++**+              
                    ?####S+SS#S####S+****++                 ?#SSSSS############## +**+              
                    ?####S?SSSS####S*+****+                 ?#SSSSS##############++**+              
                    *####SS#? ######*+****+                 ?#SSSSS######S??#####++**+              
                    *#####S#S:SS####*+****+                 ?#SSSS######S+*S#####*+**+              
                    +##?S#SSS*SS####?+****+                 %#SSSS#######S#####%S*+**+              
                    +S#?S#SSS#SSS###%+*****+               :?*#SSS############S+%***++              
                     %@%###SSSSSSS###++****+                 ?#SSS############?%S+**+               
                     ?@####SS#SSSSS##? +****+            + * SSSSS######SS####S#%+**+               
                     *#####? %#SSSSS##+ +****+            *SS#SSS######?+?######?+**+               
                     +#%%%#%: #SSSSSS#%   +***+          +S#S%#SS#####S*%#######*+**+               
                     +S?  #@?:?SSSSSS##%    ++*++++  ++  %##%:%#S####*?%++%####S+**+                
                      ?@?*@#S:+#SSSSS%%#%     +++++++   %#SS#* ###### *##S#####?+**+                
                      +#SS###+%#SSSS%*%S#%+:          +%#SSSSS?%S###@ *@#######*+**+                
                       %@####SSSSSSS**SSS#S?+      :+**%SSSSS#%++?##%+S#######S*+*++                
                       *#######SSSSSS?%#SSS#S%: *+:  ?**SSSSSS##?  #* ########?+**+                 
                       :%@@#####SSSSS##SSSS###*+S%%S%SS?SSSSS##S *S##:?####S +***++                 
                        *S?%#####SSSSSSSSSS??%#S%##S#SSSSSSSS####@###%%#####%++**+                  
                         *+*######SSS*S#SSS??%#S SSSSSSSSSSS##########@#S###S*+*++                  
                         +#+#######SS*SSSSS#SS## ?#SSSSSSS############%:*###?+**+                   
                          ?+%#######S%SSSSS+?:*#??#SSSSSSS###########S:?##@S*+*+                    
                           ?###########SSSS:%% %SSSSSSS##?%###############?*+*++                    
                           +S##%#######*?#S+S##?+SSSS####%*###*%#########S++**+                     
                            +#@??#####%?#####***S#########*##?*##########%+**+                      
                             +?#?##S###SS###S,%##############%##########%+**+                       
                              +####??#@?:S##?  %#######################%*+*++                       
                               *S##%*S@%: ####*+####################?S%++*+                         
                                +?#?+%##S*###%+S###################* ?+**+                          
                                 +?#SS#############################+***++                           
                                   *S@#S?##############?%#########%***++                            
                                    +?S#*#######S**%###*S###*+##?*++++                              
                                      +?%########SS####%##### ?*+++++                               
                                        +*%S###########@###S%**++++                                 
                                           ++?%?%#SS#SS%%?*+++++                                    
                                               +++  *++++++++  
      
